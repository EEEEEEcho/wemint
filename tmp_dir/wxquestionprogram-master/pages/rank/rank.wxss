   .tr {
    display: flex;
    width: 100%;
    justify-content: center;
    height: 2rem;
    align-items: center;
   }
   .td {
    font-size: 30rpx;
    width:40%;
    justify-content: center;
    text-align: center;
   }
   .bg-w{
        font-size: 30rpx;
        background: snow;
   }
   .bg-g{
    font-size: 30rpx;
    background: #E6F3F9;
   }
   .th {
    width: 40%;
    justify-content: center;
    background: #af1616;
    color: #fff;
    display: flex;
    height: 2rem;
    align-items: center;
    font-size: 32rpx;
    font-weight: bold;
   }
