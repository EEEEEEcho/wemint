.li__hd {
    width: 220rpx;
    height: 220rpx;
    margin-right: 20rpx;
}

.li__hd image {
    width: 100%;
    height: 100%;
}

.li {
    background-color: #fff;
    padding: 20rpx;
    border-bottom: 1px solid #e6e6e6;
    margin-bottom: 20rpx;
}

.li .tit {
    font-size: 28rpx;
}

.li .desc {
    font-size: 24rpx;
    color: #555;
}

.star {
    margin-top: 10rpx;
    display: flex;
    font-size: 24rpx;
}

.star-item {
    display: inline-block;
    width: 28rpx;
    height: 28rpx;
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
}

.weui-media-box__title {
    font-size: 26rpx;
    line-height: 20px;
    margin-bottom: 4px;
}

.oldprice {
    font-size: 20rpx;
    color: #ccc;
    text-decoration: line-through;
    margin-left: 10px;
}

.nowprice {
    font-size: 30rpx;
    color: #e4393c;
}

.weui-media-box__hd_in-appmsg {
    width: 120rpx;
    height: 120rpx;
    border: 1px solid #f2f2f2;
}

.title-group .weui-media-box__title {
    margin-bottom: 5rpx;
}

.title-group .weui-media-box__desc {
    margin-bottom: 5rpx;
}

.sl {
    color: #ccc;
    padding-top: 4px;
}

.panel-tit {
    padding: 30rpx 30rpx 0 30rpx;
    font-size: 28rpx;
}

.weui-panel .star {
    margin-top: 0;
}

.number-box {
    display: inline-block;
    vertical-align: middle;
}

.number {
    width: 100px;
}

.carbtn {
    margin-left: 10rpx;
    vertical-align: middle;
    display: inline-block;
    width: 60rpx;
    height: 60rpx;
    background-size: 100%;
    background-repeat: no-repeat;
    background-image: url(data:image/png;base64,);
}

.weui-media-box::before {
    left: 0;
}