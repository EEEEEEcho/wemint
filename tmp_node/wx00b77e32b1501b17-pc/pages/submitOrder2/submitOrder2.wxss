.cardbox {
    position: relative;
    margin-top: 15px;
    margin-left: 10px;
    margin-right: 10px;
    margin-bottom: 15px;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    box-shadow: 0 0 10px rgba(0,0,0,0.05);
    overflow: visible;
}

.cardbox:before,.checkbox_box:before {
    content: "";
    position: absolute;
    width: 60rpx;
    height: 85rpx;
    border-top: none;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
    right: 20rpx;
    left: auto;
    top: -0.37rem;
}

.fs40 {
    font-size: 40rpx;
}

.fr {
    float: right;
}

.cardbox:after {
    content: "";
    position: absolute;
    left: 0;
    right: 0;
    bottom: -10px;
    height: 10px;
    border-bottom: none;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
    background-repeat: no-repeat;
}

.card-top:after,.card-top:before {
    position: absolute;
    width: 16px;
    height: 16px;
    border-radius: 50%;
    background-color: #f4f4f4;
    content: "";
    z-index: 9;
    bottom: -8px;
}

.card-top:after {
    right: -8px;
}

.card-top:before {
    left: -8px;
}

.weui-panel__hd {
    display: flex;
    justify-content: space-between;
}

.hd_icon {
    width: 40rpx;
    height: 40rpx;
    margin-right: 10rpx;
}

.checkbox_box {
    position: relative;
    margin-top: 15px;
    margin-left: 10px;
    margin-right: 10px;
    margin-bottom: 15px;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    box-shadow: 0 0 10px rgba(0,0,0,0.05);
    overflow: visible;
}