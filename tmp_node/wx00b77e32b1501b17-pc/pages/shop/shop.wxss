.search {
    display: flex;
    background-color: #fa665b;
    color: #fff;
    padding: 14rpx 0;
}

.search-inpbox {
    flex: 1;
    background-color: #fff;
    border-radius: 80rpx;
    height: 70rpx;
    padding-left: 10px;
}

.search-inpbox input {
    height: 70rpx;
    color: #555;
}

.search-r {
    background-color: #fff;
    border-radius: 6rpx;
    width: 100rpx;
    text-align: center;
    margin: 0 10rpx;
}

.search-r image {
    width: 40rpx;
    height: 40rpx;
    margin-top: 16rpx;
}

.search-l {
    width: 120rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    text-align: center;
    height: 70rpx;
    line-height: 70rpx;
    margin: 0 10rpx;
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: 20rpx;
    background-position: right center;
    padding-right: 20rpx;
}

.li__hd {
    width: 220rpx;
    height: 220rpx;
    margin-right: 20rpx;
}

.li__hd image {
    width: 220rpx;
    height: 100%;
}

.li {
    background-color: #fff;
    padding: 20rpx;
    border-bottom: 1px solid #e6e6e6;
    margin-bottom: 20rpx;
}

.li .tit {
    font-size: 28rpx;
}

.li .desc {
    font-size: 24rpx;
    color: #555;
}

.star {
    margin-top: 10rpx;
    display: flex;
    font-size: 24rpx;
}

.star-item {
    display: inline-block;
    width: 28rpx;
    height: 28rpx;
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
}

.select-group {
    margin-top: 20rpx;
    background-color: #fff;
    text-align: center;
    border-top: 1px solid #e6e6e6;
    border-bottom: 1px solid #e6e6e6;
}

.select-group .weui-flex__item {
    height: 70rpx;
    line-height: 70rpx;
    border-right: 1px solid #e6e6e6;
}

.select-group .weui-flex__item::after {
    display: inline-block;
    width: 22rpx;
    height: 22rpx;
    vertical-align: middle;
    content: "";
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: 100%;
    background-position: center center;
    margin-left: 6px;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}