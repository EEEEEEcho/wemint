<view class="tabbar {{pageData.Fixed==='yes'?'fixed':''}}">
  {{pageData.Item.Fixed}}
  <navigator class="tabbar__item" url="{{item.Link}}" wx:for="{{pageData.Item}}">
        <view class="tabbar__icon">
            <image src="{{item.Img}}"></image>
        </view>
        <view class="tabbar__label">{{item.DisplayText}}</view>
    </navigator>
</view>
