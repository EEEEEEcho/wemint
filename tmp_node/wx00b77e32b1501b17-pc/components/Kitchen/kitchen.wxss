.kitchenbox {
    display: flex;
    position: relative;
    height: 375rpx;
    overflow: hidden;
}

.kitchenbox navigator {
    display: block;
}

.kitchenbox::after {
    position: absolute;
    left: 50%;
    top: 0;
    bottom: 0;
    border-right: 1px solid #f5f5f5;
    content: "";
}

.k_big,.k_small {
    position: relative;
    flex: 1;
    box-sizing: border-box;
}

.k_big image {
    width: 100%;
}

.k_small image {
    width: 100%;
}

.k_big navigator {
    height: 375rpx;
}

.k_big navigator image {
    height: 100%;
}

.k_small navigator {
    position: relative;
    height: 125rpx;
}

.k_small navigator,.k_big navigator {
    border-bottom: 1px solid #f5f5f5;
}

.k_small navigator image {
    height: 100%;
}

.kitchenbox.sleft {
    flex-direction: row-reverse;
}