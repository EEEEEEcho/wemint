<view class="kitchenbox  {{pageData.ShowWay==='left'?'':'sleft'}}">
    <view class="k_big">
        <navigator url="{{pageData.Item[0].Link}}">
            <image mode="widthFix" src="{{pageData.Item[0].Img}}"></image>
        </navigator>
    </view>
    <view class="k_small">
        <navigator wx:if="{{index>0}}" wx:for="{{pageData.Item}}">
            <image mode="widthFix" src="{{item.Img}}"></image>
        </navigator>
    </view>
</view>
