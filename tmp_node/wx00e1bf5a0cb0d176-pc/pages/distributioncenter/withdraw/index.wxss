page {
    background-color: #f4f4f4;
}

.container-beDistribution .beMember {
    position: relative;
    height: 180rpx;
    width: 100%;
}

.container-beDistribution .beMember .beMember-img {
    height: 180rpx;
    width: 100%;
}

.container-beDistribution .beMember .beMember-img image {
    height: 180rpx;
    width: 100%;
}

.container-beDistribution .beMember .conRule {
    display: flex;
    padding-left: 50rpx;
    position: absolute;
    top: 50rpx;
}

.container-beDistribution .beMember .conRule .rule-left .other-icon {
    font-size: 90rpx;
    color: rgba(255,255,255,.5);
    margin-top: 6rpx;
    margin-right: 26rpx;
}

.container-beDistribution .beMember .conRule .rule-right .rule1 {
    font-size: 36rpx;
    color: #fff;
    font-weight: bold;
    margin-bottom: 22rpx;
}

.container-beDistribution .beMember .conRule .rule-right .rule2 {
    font-size: 25rpx;
    color: #fff;
}

.container-beDistribution .condition {
    height: 120rpx;
    background-color: #fff;
    padding: 0 26rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.container-beDistribution .condition .condition-left {
    display: flex;
    align-items: center;
}

.container-beDistribution .condition .condition-left .circle {
    width: 10rpx;
    height: 10rpx;
    background-color: #ff7200;
    margin-right: 14rpx;
    border-radius: 50%;
}

.container-beDistribution .condition .condition-left .condition-con {
    font-size: 30rpx;
    color: #333;
}

.container-beDistribution .condition .condition-left .condition-con .price {
    color: #ff7200;
    font-weight: bold;
    padding: 0 6rpx;
}

.container-beDistribution .condition .condition-right {
    border: 1rpx solid #ff7200;
    width: 160rpx;
    height: 66rpx;
    line-height: 66rpx;
    text-align: center;
    font-size: 28rpx;
    color: #ff7200;
    border-radius: 33rpx;
}

.container-distribution {
    display: flex;
    flex-direction: column;
    box-sizing: border-box;
}

.container-distribution .commission-info {
    height: 400rpx;
    text-align: center;
    background-color: #ff4c5b;
    width: 100%;
}

.container-distribution .commission-info .commission-price {
    margin-top: 66rpx;
    font-size: 100rpx;
    line-height: 100rpx;
    color: #fff;
    width: 100%;
}

.container-distribution .commission-info .commission-name {
    font-size: 30rpx;
    line-height: 30rpx;
    color: rgba(255,255,255,0.65);
    margin-top: 22rpx;
}

.container-distribution .commission-info .withdrawal {
    margin-top: 36rpx;
    display: flex;
    justify-content: center;
}

.container-distribution .commission-info .withdrawal .withdrawal-btn {
    width: 260rpx;
    height: 88rpx;
    line-height: 88rpx;
    background-color: #fff;
    font-size: 34rpx;
    color: #ff7200;
    border-radius: 50rpx;
}

.container-distribution .withdrawal-msg {
    display: flex;
    flex-direction: column;
}

.container-distribution .withdrawal-msg .withdrawaling,.container-distribution .withdrawal-msg .pending-amount,.container-distribution .withdrawal-msg .nav,.container-distribution .team-btn .team-nav,.container-distribution .popularize-btn .popularize-nav {
    padding: 0 26rpx;
    display: flex;
    justify-content: space-between;
    height: 96rpx;
    line-height: 96rpx;
    background-color: #fff;
}

.container-distribution .withdrawal-msg .withdrawaling .w-left text,.container-distribution .withdrawal-msg .pending-amount .pending-left text,.container-distribution .withdrawal-msg .widthdrawal-left text,.container-distribution .withdrawal-msg .commission-left text,.container-distribution .withdrawal-msg .withdrawaling .w-right,.container-distribution .withdrawal-msg .pending-amount .pending-right,.container-distribution .withdrawal-msg .widthdrawal-right text,.container-distribution .withdrawal-msg .commission-right text,.container-distribution .team-btn .team-left text,.container-distribution .team-btn .team-right text,.container-distribution .popularize-btn .popularize-left text {
    font-size: 30rpx;
    color: #333;
}

.w-left,.pending-left,.widthdrawal-left,.widthdrawal-right,.commission-left,.commission-right,.team-left,.team-right,.popularize-left {
    display: flex;
}

.container-distribution .withdrawal-msg .other-icon,.container-distribution .team-btn .other-icon,.container-distribution .popularize-btn .other-icon {
    font-size: 40rpx;
    margin-right: 20rpx;
}

.container-distribution .withdrawal-msg .widthdrawal-right .icon-right,.container-distribution .withdrawal-msg .commission-right .icon-right,.container-distribution .team-btn .team-right .icon-right,.container-distribution .popularize-btn .popularize-right .icon-right {
    font-size: 26rpx;
    margin-left: 5rpx;
}

.container-distribution .my-team,.container-distribution .my-popularize,.referrer-info .referrer-title {
    height: 80rpx;
    line-height: 80rpx;
    padding: 0 26rpx;
    font-size: 26rpx;
    color: #666;
}

