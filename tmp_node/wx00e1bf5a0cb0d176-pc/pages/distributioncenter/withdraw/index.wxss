}

.referrer-info .referrer {
    display: flex;
    align-items: center;
    background: #fff;
    height: 96rpx;
    padding-left: 26rpx;
}

.referrer image {
    width: 68rpx;
    height: 68rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.referrer .referrer-name {
    font-size: 26rpx;
    color: #666;
}

.withdrawal-record {
    margin-top: 20rpx;
}

