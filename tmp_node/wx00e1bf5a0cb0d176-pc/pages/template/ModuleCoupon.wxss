ul,li {
    list-style: none;
}

a {
    text-decoration: none;
}

.flexBox {
    display: box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
}

.flexDirectionRow {
    flex-direction: row;
}

.flexDirectionColumn {
    flex-direction: column;
}

.overflowAutos {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.flexJustify {
    -webkit-box-pack: center;
    justify-content: center;
}

.flexJustifyBetween {
    justify-content: space-between;
}

.flexAlign {
    -webkit-box-align: center;
    align-items: center;
}

.boxSizingBox {
    box-sizing: border-box;
}

.flex1 {
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
    flex: 1;
}

.overflow1 {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
}

.overflow2 {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.fixed-top {
    position: fixed!important;
    top: 0;
    width: 100%;
    box-sizing: border-box;
}

.module-item {
    position: relative;
}

.padding-lr {
    padding-left: 20rpx;
    padding-right: 20rpx;
}

.padding-tb {
    padding-top: 6rpx;
    padding-bottom: 6rpx;
}

.ModuleCoupon .ModuleCoupon-more {
    display: flex;
    align-items: center;
    justify-content: center;
    height: 200rpx;
    width: 200rpx;
    padding: 0rpx 50rpx;
    margin: 20rpx 0rpx;
    color: #ff7200;
}

.ModuleCoupon .ModuleCoupon-more .iconfont {
    font-size: 50rpx;
}

.ModuleCoupon {
    margin-bottom: 20rpx;
    background-color: #fff;
}

.ModuleCoupon .container {
    display: flex;
    flex-direction: row;
}

.ModuleCoupon .container .item {
    height: 200rpx;
    margin-bottom: 20rpx;
    box-sizing: border-box;
    padding-right: 20rpx;
    margin-top: 20rpx;
}

.ModuleCoupon .container .item:first-child {
    margin-left: 20rpx;
}

.ModuleCoupon .container .item .couponinfo {
    border-radius: 2px;
    display: flex;
    flex-direction: column;
    height: 100%;
    box-sizing: border-box;
    width: 500rpx;
    box-shadow: 0 0 20rpx rgba(0,0,0,.1);
}

.ModuleCoupon .container .item .couponinfo .col1 {
    width: 100%;
    height: 140rpx;
    color: #fff;
    border-top-left-radius: 2px;
    border-top-right-radius: 2px;
}

.ModuleCoupon .container .item-full .couponinfo .col1 {
    flex: 0 0 30%;
}

.ModuleCoupon .container .item .couponinfo .col1 .coupon-money .coupon-symbol {
    font-size: 22rpx;
}

.ModuleCoupon .container .item .couponinfo .col1 .coupon-money {
    line-height: 38rpx;
}

.ModuleCoupon .container .item .couponinfo .col1 .coupon-avail {
    font-size: 22rpx;
    margin-top: 6rpx;
}

.ModuleCoupon .container .item .couponinfo .col2 {
    width: 100%;
    height: 58rpx;
    line-height: 58rpx;
    color: #999;
    background-color: #fff;
    position: relative;
    font-size: 24rpx;
    padding: 0 26rpx;
    box-sizing: border-box;
}

.ModuleCoupon .container .item .couponinfo .col2 text {
    font-size: 24rpx;
}

.ModuleCoupon .container .item .couponinfo .col2::after {
    content: '';
    position: absolute;
    width: 100%;
    height: 1px;
    top: -6rpx;
    left: 0;
    border-top: 12rpx dotted #fff;
    z-index: 55;
}

.container .item .couponinfo .col1 .moneyBox {
    flex: 1;
    padding-left: 26rpx;
    box-sizing: border-box;
    height: 140rpx;
    border-right: 1px dashed rgba(255,255,255,0.5);
    display: flex;
    justify-content: center;
}

.container .item .couponinfo .col1 .moneyBox .astrict {
    font-size: 11px;
    display: inline-block;
}

.container .item .couponinfo .col1 .moneyBox .coupon-money .moneyText {
    font-size: 64rpx;
}

.container .item .couponinfo .col1 .moneyBox .coupon-money .unit {
    font-size: 24rpx;
}

.container .item .couponinfo .col1 .typeBox {
    flex: 0 0 240rpx;
}

.container .item .couponinfo .col1 .typeBox .unitText {
    font-size: 28rpx;
}

.container .item .couponinfo .col1 .typeBox .coupon-getbtn {
    width: 156rpx;
    height: 44rpx;
    font-size: 24rpx;
    text-align: center;
    line-height: 44rpx;
    background-color: #fff;
    border-radius: 44rpx;
    margin-top: 12rpx;
}