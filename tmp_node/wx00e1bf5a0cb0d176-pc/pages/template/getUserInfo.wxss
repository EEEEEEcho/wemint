.panel-mark {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0,0,0,0.5);
    z-index: 1000000;
}

.panel-getUserInfo {
    width: 620rpx;
    height: 528rpx;
    border-radius: 16rpx;
    text-align: center;
    background: #fff;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    z-index: 1000001;
}

.panel-getUserInfo .panel-title {
    font-size: 34rpx!important;
    color: #000;
    font-weight: 500;
    height: 98rpx;
    line-height: 98rpx;
}

.panel-getUserInfo .panel-tips {
    padding: 40rpx 62rpx 0;
    text-align: left;
}

.panel-getUserInfo .panel-tips .img {
    width: 70rpx;
    height: 70rpx;
    margin: 0 auto;
    margin-bottom: 12rpx;
}

.panel-getUserInfo .panel-tips .img image {
    width: 70rpx;
    height: 70rpx;
    border-radius: 50%;
}

.panel-getUserInfo .panel-tips .app-title {
    font-size: 30rpx;
    color: #000;
    height: 112rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.panel-getUserInfo .panel-tips .allow-tips {
    padding-top: 32rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.panel-getUserInfo .panel-tips .allow-tips .circle {
    height: 10rpx;
    width: 10rpx;
    border-radius: 50%;
    background: #898989;
    margin-right: 20rpx;
}

.panel-getUserInfo .panel-tips .allow-tips .ebook {
    font-size: 26rpx;
    color: #898989;
}

.panel-getUserInfo .btn-groups {
    display: flex;
    flex-direction: row;
    position: absolute;
    bottom: 0;
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
}

.panel-getUserInfo .btn-groups::before {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    border-top: 1rpx solid #ddd;
    content: '';
    transform-origin: 0 0;
}

.panel-getUserInfo .btn-groups button {
    font-size: 34rpx;
    color: #2fb823;
    background: #fff;
    border-radius: 16rpx;
    height: 88rpx;
    margin-top: 1rpx;
    width: 100%;
}

.button-hover[type=primary] {
    background: transparent;
    color: #2fb823;
}

button::after {
    border: none;
}

