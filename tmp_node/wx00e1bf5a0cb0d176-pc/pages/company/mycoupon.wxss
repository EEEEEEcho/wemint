page {
    background-color: #f4f4f4;
}

.container {
}

.navbar {
    flex: none;
    display: flex;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 88rpx;
    background: #fff;
    z-index: 10;
}

.navbar .item {
    position: relative;
    flex: auto;
    font-size: 30rpx;
    text-align: center;
    height: 88rpx;
    line-height: 88rpx;
    color: #666;
}

.navbar .item.active {
    color: #ffa72a;
}

.navbar .item.active::after {
    content: "";
    display: block;
    position: absolute;
    bottom: 1rpx;
    left: 50%;
    transform: translateX(-50%);
    width: 120rpx;
    height: 4rpx;
    border-radius: 8px;
    background: #ffa72a;
}

.couponshow {
    background-color: #fff;
    padding: 25px 10px 0;
}

.color999 {
    color: #999!important;
}

.coupon-title {
    text-align: center;
    margin: 0 auto;
    font-size: 40rpx;
    padding-bottom: 50rpx;
}

.coupon-item {
    position: relative;
    margin: 30rpx 26rpx;
    border-radius: 8rpx;
    overflow: hidden;
    background-color: #fff;
}

.coupon-item .item-head {
    display: flex;
    flex-direction: row;
    padding: 26rpx 26rpx 26rpx 0;
    box-sizing: border-box;
    border-bottom: 1px dashed #ffcca2;
    align-items: center;
}

.coupon-item .item-head.noborder {
    border: none;
}

.coupon-item .item-head-left {
    display: flex;
    width: 240rpx;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.coupon-item .item-head-left .code-item {
    color: #ff7200;
    display: flex;
    align-items: baseline;
    justify-content: center;
    margin-bottom: 30rpx;
}

.coupon-item .item-head-left .code-item .money-num-text {
    display: flex;
    align-items: center;
    justify-content: center;
    line-height: 38rpx;
}

.coupon-item .item-head-left .code-item .unit {
    display: inline;
    font-size: 26rpx;
}

.coupon-item .item-head-left .use-type-text {
    font-size: 22rpx;
    color: #333;
}

.item-head-right {
    flex: 1;
    display: flex;
    justify-content: flex-start;
    flex-direction: column;
}

.item-head-right .title-text {
    font-size: 22rpx;
    color: #666666;
    margin-bottom: 8rpx;
}

.item-head-right .use-money {
    font-size: 26rpx;
    color: #333;
    margin-bottom: 16rpx;
}

.item-head-right .time {
    font-size: 22rpx;
    color: #666666;
    margin-bottom: 6rpx;
}

.btn-list {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.btn-list view {
    position: relative;
    flex: 1;
    display: flex;
    align-items: center;
    justify-content: center;
    height: 78rpx;
    color: #ff7200;
    font-size: 28rpx;
}

.btn-list .product-list-btn::before {
    content: '';
    position: absolute;
    right: 0;
    width: 1px;
    height: 50rpx;
    top: 14rpx;
    border-right: 1px dashed #ffcca2;
}

.common-coupon-btn {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 646rpx;
    height: 80rpx;
    background-color: #ff7200;
    border-radius: 80rpx;
    font-size: 30rpx;
    color: #fff;
    font-weight: 700;
    margin: 0 auto;
}

.coupon-list {
    width: 100%;
    padding-top: 44px;
}

.used {
    color: #999!important;
}

.item-title {
    border-top-left-radius: 10rpx;
    border-top-right-radius: 10rpx;
}

.item-title .title {
    color: #ccc;
    font-size: 26rpx;
}

.coupon-time {
    display: flex;
    align-items: center;
    height: 114rpx;
    background: #fff;
    padding: 0 26rpx;
    border-bottom-left-radius: 10rpx;
    border-bottom-right-radius: 10rpx;
}

.coupon-time .time {
    flex: 0 0 70%;
    font-size: 22rpx;
    color: #999;
    padding-top: 10rpx;
}

.coupon-time .status {
    flex: 1;
    text-align: right;
    font-size: 22rpx;
    color: #999;
}

.couponGetOver {
    width: 100%;
    position: fixed;
    top: 200rpx;
    left: 50%;
    transform: translateX(-50%);
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    margin-top: 50px;
}

.couponGetOver image {
    display: inline-block;
    width: 300rpx;
}

.couponGetOver text {
    font-size: 24rpx;
    color: #999;
}

.titleMoney {
    display: block;
    font-size: 48rpx;
    margin-bottom: 10rpx;
    font-weight: normal;
}

.item-title .title .titleText {
    display: inline-block;
    font-size: 26rpx;
}

.title {
    margin-left: 160rpx;
}

.title text {
    display: block;
}

.title-coupon {
    font-size: 36rpx;
    color: #ffa72a;
}

.title-tit {
    font-size: 22rpx;
    color: #999;
    padding-top: 10rpx;
}

.coupon-time .jine {
    font-size: 22rpx;
    color: #999;
}

.coupon-time .jine text {
    display: block;
}

.coupon-time .use {
    width: 164rpx;
    height: 60rpx;
    background: #ffa72a;
    color: #fff;
    font-size: 26rpx;
    margin-left: 138rpx;
    line-height: 60rpx;
    text-align: center;
    border-radius: 30rpx;
}

.title-img {
    position: absolute;
    top: 0;
    right: 0;
    width: 130rpx;
    height: 130rpx;
}

.title-img image {
    width: 100%;
    height: 130rpx;
}

.container .couponEnter {
    height: 120rpx;
    width: 120rpx;
    position: fixed;
    bottom: 80rpx;
    right: 50rpx;
}

.container .couponEnter image {
    height: 120rpx;
    width: 120rpx;
}

.common-coupon-btn-bg {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    width: 100%;
    height: 120rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.common-coupon-btn-bg.active {
    background-color: #f4f4f4;
}