.pagecontent .img {
    margin-top: 54rpx;
}

.pagecontent .processing {
    width: 110rpx;
    height: 110rpx;
}

.pagecontent .text-tip {
    margin-top: 20rpx;
}

.pagecontent .text-tip .tip-one {
    font-size: 36rpx;
    line-height: 36rpx;
    color: #666;
    font-weight: 500;
}

.pagecontent .text-tip .tip-two {
    font-size: 24rpx;
    line-height: 24rpx;
    color: #999;
    margin-top: 16rpx;
}