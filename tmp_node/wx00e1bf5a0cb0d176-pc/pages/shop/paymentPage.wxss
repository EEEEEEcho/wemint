}

.pagecontent .v5_right {
    display: flex;
    align-items: center;
}

.pagecontent .v5_right_btn {
    width: 120rpx;
    height: 60rpx;
    font-size: 28rpx;
    color: #fff;
    border-radius: 999rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.pagecontent .v6 {
    height: 60rpx;
