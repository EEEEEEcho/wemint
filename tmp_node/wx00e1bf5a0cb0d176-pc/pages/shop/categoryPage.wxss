.search-product {
    height: 106rpx;
    width: 100%;
    padding: 20rpx 26rpx;
    box-sizing: border-box;
    background-color: #fff;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 999;
    border-bottom: 1px solid #f4f4f4;
}

.search-product .formSearch {
    border-radius: 40rpx;
    background-color: #fff;
    width: 100%;
    height: 80rpx;
}

.searchinp {
    width: 693rpx;
    height: 66rpx;
    line-height: 66rpx;
    border-radius: 40rpx;
    padding-left: 80rpx;
    box-sizing: border-box;
    font-size: 28rpx;
    background-color: #f4f4f4;
}

.searchimg {
    position: relative;
    display: flex;
    flex-direction: row;
    align-items: center;
    width: 44rpx;
    height: 70rpx;
    top: -68rpx;
    left: 32rpx;
}

.searchimg .iconfont {
    font-size: 40rpx;
    color: #999;
}

.nothing {
    width: 100%;
    position: fixed;
    top: 200rpx;
    left: 50%;
    transform: translateX(-50%);
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    margin-top: 50px;
}

.nothing image {
    display: inline-block;
    width: 300rpx;
}

.nothing .nothing-title {
    font-size: 24rpx;
    color: #999;
}

.container-box {
    display: flex;
    flex-direction: row;
    width: 100%;
    margin-top: 106rpx;
}

.container-box .classTabList {
    width: 164rpx;
    display: flex;
    flex-flow: column;
    background-color: #fbfbfb;
    position: fixed;
    left: 0;
    z-index: 999;
}

.container-box .classTabList .classTabItem {
    width: 164rpx;
    height: 100rpx;
    text-align: center;
    color: #333;
    font-size: 24rpx;
    box-sizing: border-box;
    position: relative;
    z-index: 998;
    padding: 0 24rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.container-box .classTabList .classTabItem .itemName {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    width: 110rpx;
    text-align: left;
}

.container-box .classTabList .classTabItem.isSelected {
    font-size: 24rpx;
    color: #FF7200;
    background-color: #fff;
    width: 164rpx;
    box-sizing: border-box;
    font-weight: 700;
}

.first-classify {
    background-color: #fff;
    width: 100%;
}

.first-classify .box-content {
    padding: 20rpx;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}

.first-classify .box-content .commodity {
    text-align: center;
    margin-bottom: 40rpx;
    flex: 0 0 33.3%;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.first-classify .box-content .commodity-img {
    width: 140rpx;
    height: 140rpx;
    margin: 0 auto;
}

.first-classify .box-content .commodity-img image {
    height: 140rpx;
    width: 140rpx;
}

.first-classify .box-content .commodity-name {
    font-size: 24rpx;
    color: #666;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 180rpx;
    margin-top: 8rpx;
}

.second-classify {
    background: #fff;
    padding: 20rpx 0 20rpx 20rpx;
    position: fixed;
    right: 0;
}

.second-classify .itemName {
    display: flex;
    flex-direction: row;
    width: 310rpx;
    height: 54rpx;
    margin: 24rpx auto;
    justify-content: center;
}

.second-classify .itemName .line {
    width: 32rpx;
    height: 2rpx;
    background-color: #ddd;
    position: relative;
    top: 16rpx;
}

.second-classify .itemName text {
    color: #666;
    font-size: 24rpx;
    margin: 0 20rpx;
}

.second-classify .onlyOne .porduct {
    font-size: 24rpx;
    color: #666;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    margin-right: 20rpx;
}

.second-classify .onlyOne .porduct .product-box {
    width: 540rpx;
    height: 180rpx;
    border-radius: 6rpx;
}

.second-classify .onlyOne .porduct image {
    width: 540rpx;
    height: 180rpx;
    border-radius: 6rpx;
}

.second-classify .haveTwo,.second-classify .haveThree {
    padding-bottom: 100rpx;
}

.second-classify .firstClassifyImg {
    width: 540rpx;
    height: 180rpx;
    border-radius: 6rpx;
}

.second-classify .firstClassifyImg image {
    width: 546rpx;
    height: 180rpx;
    border-radius: 6rpx;
}

.second-classify .haveTwo .product-box {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    margin-right: 20rpx;
}

.second-classify .haveTwo .product-box .product {
    width: 140rpx;
    text-align: center;
    margin-bottom: 26rpx;
    flex: 0 0 33.3%;
}

.second-classify .haveTwo .product-box .product image {
    width: 140rpx;
    height: 140rpx;
    margin-bottom: 8rpx;
}

.second-classify .haveTwo .product-box .product .productName {
    font-size: 24rpx;
    color: #666;
}

.second-classify .haveThree .product-onecss {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    margin-right: 20rpx;
}

.second-classify .haveThree .product-onecss .product,.second-classify .haveThree .product-twocss .product-twocss-box {
    width: 140rpx;
    text-align: center;
    margin-bottom: 26rpx;
    flex: 0 0 33.3%;
}

.second-classify .haveThree .product-onecss .product image {
    width: 140rpx;
    height: 140rpx;
    margin-bottom: 8rpx;
}

.second-classify .haveThree .product-twocss {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    margin-right: 20rpx;
}

.second-classify .haveThree .product-twocss image {
    width: 140rpx;
    height: 140rpx;
    margin-bottom: 8rpx;
}

.second-classify .haveThree .product-twocss .product-name,.second-classify .haveThree .product-onecss .product .product-name {
    font-size: 24rpx;
    color: #666;
    text-align: center;
    width: 180rpx;
    box-sizing: border-box;
}