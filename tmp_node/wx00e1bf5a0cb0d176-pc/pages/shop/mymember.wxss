.memberWrapper .headerWrapper {
    display: flex;
    flex-direction: column;
    align-items: center;
    padding: 0 26rpx;
    background-color: #fff;
}

.headerWrapper .imgMember {
    width: 104rpx;
    height: 104rpx;
    overflow: hidden;
    border-radius: 50%;
    margin-top: 50rpx;
}

.icon-huiyuanyiji {
    color: #fcd23d;
    font-size: 40rpx;
    padding-right: 10rpx;
}

.yongjin {
    display: flex;
    flex: row;
    align-items: center;
    width: 100%;
}

.yongjin-l {
    width: 50%;
    justify-content: center;
    text-align: center;
    margin-bottom: 30rpx;
    margin-top: 20rpx;
}

.yongjin-l .yongjin-con {
    font-size: 24rpx;
    color: #999;
    padding-bottom: 20rpx;
}

.headerWrapper .imgMember image {
    width: 100%;
    height: 100%;
}

.yongjin-l .yongjin-txt {
    font-size: 30rpx;
    color: #000003;
}

.memberName {
    display: inline-block;
    margin: 30rpx 0;
    font-size: 30rpx;
    color: #333;
}

.memberSeries {
    font-size: 26rpx;
    color: #333;
}

.headerWrapper .memberMessage {
    flex: 1;
    display: flex;
    flex-direction: row;
}

.contentWrapper {
    margin-top: 20rpx;
    background-color: #fff;
}

.contentWrapper .timeTitle {
    height: 88rpx;
    display: flex;
    align-items: center;
    font-size: 26rpx;
    color: #999;
    padding: 0 26rpx;
}

.time {
    font-size: 24rpx;
    color: #999;
}

.icon-iconfont-yongjin {
    color: #f9ad28;
    margin-right: 10rpx;
}

.Money {
    color: #333;
    font-size: 28rpx;
}

.contributionMoney {
    padding: 0 26rpx;
    height: 96rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 28rpx;
    color: #333;
}