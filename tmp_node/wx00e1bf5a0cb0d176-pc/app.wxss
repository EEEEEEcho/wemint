@import "pages/template/customform.wxss";

@import "pages/common/popout/index.wxss";

@import "__wuBaseWxss__/3.wxss";

text,view {
    padding: 0;
    margin: 0;
}

page {
    display: flex;
    flex-direction: column;
    width: 100%;
    background-color: #f4f4f4;
    font-family: -apple-system-font,'Helvetica Neue',Helvetica,'Microsoft YaHei',sans-serif;
}

.container {
    display: flex;
    flex-direction: column;
    box-sizing: border-box;
}

.border1px {
    position: relative;
}

.border1px:after {
    position: absolute;
    left: 0;
    bottom: 0px;
    width: 100%;
    border-bottom: 1px solid #ddd;
    content: '';
    transform-origin: 0 0;
}

.borderTop1px {
    position: relative;
}

.borderTop1px:before {
    position: absolute;
    right: 0;
    top: 0;
    width: 100%;
    border-top: 1px solid #ddd;
    content: '';
    transform-origin: 0 0;
}

@media only screen and (-webkit-min-device-pixel-ratio:2) {
    .border1px:after {
        transform: scaleY(0.5);
    }

    .borderTop1px:before {
        transform: scaleY(0.5);
    }
}

@media only screen and (-webkit-min-device-pixel-ratio:3) {
    .border1px:after {
        transform: scaleY(0.33);
    }

    .borderTop1px:before {
        transform: scaleY(0.33);
    }
}

.clearfix::before {
    content: '';
    clear: both;
    display: block;
    height: 0;
    visibility: hidden;
}

.clearfix {
    zoom: 1;
}

.fixedTopBorder {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 0px;
    border-bottom: 1rpx solid #ddd;
    transform: scaleY(0.7);
    z-index: 999;
}

.local-btn-box {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    width: 100%;
    z-index: 1;
    opacity: 0;
}

.omit1 {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
}

.omit2 {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}

.omit3 {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 3;
    overflow: hidden;
}

.v-border {
    border: none;
    position: relative;
}

.v-border:after {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    border: 1px solid #ddd;
    box-sizing: border-box;
    width: 200%;
    height: 200%;
    transform: scale(0.5);
    transform-origin: left top;
}

.plugNav,.navShowAndHide {
    display: none!important;
}

.common-corner {
    position: relative;
}

.common-corner::before {
    content: '';
    width: 20rpx;
    height: 20rpx;
    background-color: #f4f4f4;
    border-radius: 50%;
    position: absolute;
    bottom: -10rpx;
    left: -10rpx;
}

.common-corner::after {
