<view class="k10"></view>
<view class="title">
    <view bindtap="navchange" class="title-b {{status==1?'active':''}}" data-status="1">个人收益排行榜</view>
    <view bindtap="navchange" class="title-b {{status==2?'active':''}}" data-status="2">车辆收益排行榜</view>
</view>
<view class="b-line">
    <view class="b-line-{{status==1?'a':'b'}}"></view>
    <view class="b-line-{{status==2?'a':'b'}}"></view>
</view>
<view class="k20 clear"></view>
<view wx:if="{{status==1}}">
    <view class="rank1-list" wx:if="{{usernum>0}}">
        <view class="k40"></view>
        <view class="first">
            <image class="img" src="{{user[0].headimgurl}}"></image>
        </view>
        <view class="first-img">
            <image class="img" src="/pages/img/first.png"></image>
        </view>
        <view class="k30"></view>
        <view class="name color1">{{user[0].nickname}}</view>
        <view class="k6"></view>
        <view class="shouyi">收益{{user[0].gains}}积分</view>
    </view>
    <view class="rank3-list" wx:if="{{usernum>1}}">
        <view class="k40"></view>
        <view class="second">
            <image class="img" src="{{user[1].headimgurl}}"></image>
        </view>
        <view class="second-img">
            <image class="img" src="/pages/img/second.png"></image>
        </view>
        <view class="k30"></view>
        <view class="name color2">{{user[1].nickname}}</view>
        <view class="k6"></view>
        <view class="shouyi">收益{{user[1].gains}}积分</view>
    </view>
    <view class="rank3-list" wx:if="{{usernum>2}}">
        <view class="k40"></view>
        <view class="second">
            <image class="img" src="{{user[2].headimgurl}}"></image>
        </view>
