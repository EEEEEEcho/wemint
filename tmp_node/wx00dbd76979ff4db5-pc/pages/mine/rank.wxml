        <view class="name color3">{{user[2].nickname}}</view>
        <view class="k6"></view>
        <view class="shouyi">收益{{user[2].gains}}积分</view>
    </view>
    <view class="four" wx:if="{{usernum>3}}">
        <view class="four-li">
            <view class="four-li-l">NO.4</view>
            <view class="four-li-m">
                <image class="img" src="{{user[3].headimgurl}}"></image>
            </view>
            <view class="four-li-m2">{{user[3].nickname}}</view>
            <view class="four-li-r">{{user[3].gains}}</view>
        </view>
    </view>
    <view class="four" wx:if="{{usernum>4}}">
