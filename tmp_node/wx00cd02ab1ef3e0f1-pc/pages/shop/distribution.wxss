page {
    background-color: #f4f4f4;
}

.container {
    display: flex;
    flex-direction: column;
    box-sizing: border-box;
}

.container .commission-info {
    height: 400rpx;
    text-align: center;
    background-color: #ff4c5b;
    width: 100%;
}

.container .commission-info .commission-price {
    margin-top: 66rpx;
    font-size: 100rpx;
    line-height: 100rpx;
    color: #fff;
    width: 100%;
}

.container .commission-info .commission-name {
    font-size: 30rpx;
    line-height: 30rpx;
    color: rgba(255,255,255,0.65);
    margin-top: 22rpx;
}

.container .commission-info .withdrawal {
    margin-top: 36rpx;
    display: flex;
    justify-content: center;
}

.container .commission-info .withdrawal .withdrawal-btn {
    width: 260rpx;
    height: 88rpx;
    line-height: 88rpx;
    background-color: #fff;
    font-size: 34rpx;
    color: #ff7200;
    border-radius: 50rpx;
}

.container .withdrawal-msg {
    display: flex;
    flex-direction: column;
}

.container .withdrawal-msg .withdrawaling,.container .withdrawal-msg .pending-amount,.container .withdrawal-msg .nav,.container .team-btn .team-nav,.container .popularize-btn .popularize-nav {
    padding: 0 26rpx;
    display: flex;
    justify-content: space-between;
    height: 96rpx;
    line-height: 96rpx;
    background-color: #fff;
}

.container .withdrawal-msg .withdrawaling .w-left text,.container .withdrawal-msg .pending-amount .pending-left text,.container .withdrawal-msg .widthdrawal-left text,.container .withdrawal-msg .commission-left text,.container .withdrawal-msg .withdrawaling .w-right,.container .withdrawal-msg .pending-amount .pending-right,.container .withdrawal-msg .widthdrawal-right text,.container .withdrawal-msg .commission-right text,.container .team-btn .team-left text,.container .team-btn .team-right text,.container .popularize-btn .popularize-left text {
    font-size: 30rpx;
    color: #333;
}

.w-left,.pending-left,.widthdrawal-left,.widthdrawal-right,.commission-left,.commission-right,.team-left,.team-right,.popularize-left {
    display: flex;
}

.container .withdrawal-msg .other-icon,.container .team-btn .other-icon,.container .popularize-btn .other-icon {
    font-size: 40rpx;
    margin-right: 20rpx;
}

.container .withdrawal-msg .widthdrawal-right .icon-right,.container .withdrawal-msg .commission-right .icon-right,.container .team-btn .team-right .icon-right,.container .popularize-btn .popularize-right .icon-right {
    font-size: 26rpx;
    margin-left: 5rpx;
}

.container .my-team,.container .my-popularize {
    height: 80rpx;
    line-height: 80rpx;
    padding: 0 26rpx;
    font-size: 26rpx;
    color: #666;
}

