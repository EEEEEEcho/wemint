    text-align: center;
    line-height: 200rpx;
}

.container .particulars-page .product-parameter .noParam text {
    font-size: 26rpx;
    color: #999;
}

.container .particulars-page .product-parameter .paramList {
    padding: 31rpx 0;
    font-size: 26rpx;
    display: flex;
    align-items: baseline;
}

.container .particulars-page .product-parameter .paramList .pro-name {
    color: #999;
    margin-right: 50rpx;
    flex: 20% 0 0;
}

.container .particulars-page .product-parameter .paramList .pro-content {
    color: #333;
}

.container .product-attrs .product-conTop {
    height: 170rpx;
    display: flex;
    overflow: visible;
    justify-content: space-between;
}

.container .product-attrs .product-conTop .top-left {
    position: relative;
}

.container .product-attrs .product-conTop .top-left .content {
    padding-top: 22rpx;
    margin-left: 242rpx;
}

.container .product-attrs .product-conTop .top-left .content .price {
    font-size: 30rpx;
    font-weight: bold;
    color: #333;
    padding-bottom: 6rpx;
}

.container .product-attrs .product-conTop .top-left .content .price text:nth-of-type(2) {
    font-size: 34rpx;
}

.container .product-attrs .product-conTop .close {
    padding: 10rpx 26rpx 0 0;
    color: #666;
}

.container .product-attrs .product-conTop .top-left .content .inventory,.container .product-attrs .product-conTop .top-left .content .hasChoose {
    font-size: 24rpx;
    color: #666;
}

.container .product-attrs .product-conTop .top-left .content .inventory {
    padding-bottom: 4rpx;
}

.container .product-attrs .product-conTop .top-left .content .hasChoose .message {
    color: #ff4c5b;
}

.container .product-attrs .product-conTop .img image {
    width: 196rpx;
    height: 204rpx;
    border-radius: 12rpx;
}

.container .product-attrs .product-conTop .img {
    width: 196rpx;
    height: 204rpx;
    border-radius: 12rpx;
    margin-left: 26rpx;
    position: absolute;
    left: 0;
    top: -60rpx;
    background-color: #f4f4f4;
    box-shadow: 0rpx 0rpx 20rpx rgba(51,51,51,0.4);
}

.container .product-attrs .product-conTop .close .closeIcon,.container .discountList .dis-top .markClose .closeIcon {
    font-size: 38rpx;
}

.container .product-attrs .pro-attrs {
    margin-top: 36rpx;
    padding: 0 26rpx;
    width: 650rpx;
}

.container .product-attrs .pro-attrs:nth-of-type(2) {
    margin-top: 18rpx;
}

.container .product-attrs .attr-name {
    font-size: 24rpx;
    color: #999;
    margin: 0 0 18rpx 0;
}

.container .product-attrs .pro-attrs .attr-vals {
    display: flex;
    flex-wrap: wrap;
}

.container .product-attrs .pro-attrs .attr-val {
    min-width: 124rpx;
    padding: 0 14rpx;
    height: 64rpx;
    line-height: 64rpx;
    color: #333;
    text-align: center;
    border-radius: 5rpx;
    overflow: hidden;
    font-size: 24rpx;
    border: 1px solid #ddd;
    margin-right: 20rpx;
    margin-bottom: 18rpx;
}

.container .product-attrs .pro-attrs .attr-val:last-child {
    margin-right: 0;
}

.container .product-attrs .chooseCount {
    padding: 18rpx 26rpx 0;
}

.container .product-attrs .chooseCounts {
    padding-bottom: 166rpx;
}

.container .product-attrs .chooseCount .btn {
    display: flex;
}

.container .product-attrs .chooseCount .btn .decrease,.container .product-attrs .chooseCount .btn .increase {
    width: 64rpx;
    height: 64rpx;
    text-align: center;
    line-height: 64rpx;
    color: #fff;
}

.container .product-attrs .chooseCount .btn .decrease {
    color: #666;
}

.container .product-attrs .chooseCount .btn input {
    border: 1px solid #f4f4f4;
    text-align: center;
    width: 190rpx;
    height: 60rpx;
}

.container .product-attrs .chooseCount .btn .normal {
    background-color: #ff4c5b;
    color: #fff;
}

.container .product-attrs .chooseCount .btn .disabled {
    background-color: #f4f4f4;
}

.container .product-attrs .chooseBuy {
    display: flex;
    position: fixed;
    bottom: 0;
    height: 98rpx;
    background-color: #fff;
    z-index: 100;
}

.container .product-attrs .chooseBuy .addToCart,.container .product-attrs .chooseBuy .buyNow {
    width: 375rpx;
    height: 100rpx;
    font-size: 30rpx;
    text-align: center;
    line-height: 100rpx;
}

.container .product-attrs .chooseBuy .addToCart {
    background-color: rgba(255,114,0,0.2);
    color: #ff4c5b;
}

.container .product-attrs .chooseBuy .buyNow {
    background-color: #ff4c5b;
    color: #fff;
}

.container .product-attrs {
    background-color: #fff;
    height: 840rpx;
}

.container .hideParam {
    position: fixed;
    bottom: 0;
    transform: translateY(150%);
    transition: all 0.3s linear;
    z-index: 100;
    width: 100%;
}

.container .showParam {
    position: fixed;
    bottom: 0;
    transform: translateY(0);
    transition: all 0.3s linear;
    z-index: 999;
    width: 100%;
}

.container .mark {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 10;
    background: rgba(0,0,0,0.5);
    display: none;
    transition: all 0.3s ease;
}

.container .showMark {
    display: block;
}

.footnav {
    box-shadow: 0 0 10rpx rgba(0,0,0,0.1);
    display: flex;
    background-color: #fff;
    box-sizing: border-box;
    padding-top: 0;
}

.container .footnav .footLeft {
    display: flex;
    color: #666;
    justify-content: space-around;
    align-items: center;
    height: 98rpx;
}

.container .footnav .footLeft view {
    width: 100rpx;
}

.container .footnav .footLeft .shopCar,.container .footnav .footLeft .service,.container .footnav .footLeft .favorite {
