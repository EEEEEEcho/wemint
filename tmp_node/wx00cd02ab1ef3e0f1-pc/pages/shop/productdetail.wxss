@import "UserReg.wxss";

page {
    font-family: "微软雅黑";
}

.container {
    width: 100%;
    overflow: hidden;
}

.container .imgbox {
    margin: 10px;
}

.container .imgbox image {
    width: 100%;
}

.container .particulars-text {
    padding: 20rpx 26rpx 0;
    box-sizing: border-box;
    background: #fff;
}

.container .particulars-text .title,.container .particulars-text .desc,.container .particulars-text .explain {
    display: block;
}

.container .particulars-text .particulars-title {
    font-size: 30rpx;
    color: #333;
    line-height: 48rpx;
}

.container .particulars-text .desc {
    font-size: 13px;
    color: #666;
}

.container .particulars-text .productprice .price,.container .particulars-text .productprice .oldprice {
    margin-top: 8px;
}

.container .particulars-text .productprice .prices {
    padding-top: 16rpx;
}

.container .particulars-text .productprice .price {
    color: #333;
    font-size: 34rpx;
    margin-right: 12rpx;
}

.container .particulars-text .productprice .expAndSales {
    padding: 16rpx 0 20rpx;
    display: flex;
    justify-content: space-between;
}

.container .particulars-text .productprice .price text:nth-child(2) {
    font-size: 10px;
    font-weight: bold;
}

.container .particulars-text .productprice .price text:last-child {
    font-size: 19px;
    font-weight: bold;
}

.container .particulars-text .productprice .protext {
    font-size: 24rpx;
    color: #999;
}

.container .particulars-text .productprice .oldprice {
    text-decoration: line-through;
}

.container .particulars-text .explain {
    margin-top: 25px;
    color: #d8000a;
    font-size: 12px;
}

.container .particulars-page {
    padding: 10px 0 100rpx;
    box-sizing: border-box;
}

.container .particulars-page .particulars-title {
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    display: flex;
    justify-content: space-around;
    padding: 0 26rpx;
    background-color: #fff;
    box-sizing: border-box;
}

.container .particulars-page .particulars-title .item {
    position: relative;
    flex: 0 0 20%;
    font-size: 28rpx;
    text-align: center;
    line-height: 40px;
    color: #999;
}

.container .particulars-page .particulars-title .item.active {
    color: #333;
}

.container .particulars-page .particulars-title .item.active::after {
    content: "";
    display: block;
    position: absolute;
    bottom: 1rpx;
    left: 0;
    right: 0;
    width: 100%;
    height: 4rpx;
    background: #ff4c5b;
}

.container .particulars-page .particulars-content,.container .particulars-page .product-parameter {
    width: 100%;
    background-color: #fff;
    box-sizing: border-box;
}

.container .particulars-page .particulars-content .wxParse-div {
    margin: 0!important;
    width: auto!important;
}

.container .particulars-page .particulars-content .product-information image,.container .particulars-page .particulars-content .product-information text {
    display: block;
    width: 100%;
}

.container .particulars-page .particulars-content .product-information text {
    padding: 15px 0;
    line-height: 19px;
    font-size: 13px;
    color: #666;
}

.slide-image {
    display: block;
    width: 100%;
}

.wxParse-img {
    display: block;
}

.container .product-attrs .pro-attrs .attr-vals .attr-val.active {
    border: 1px solid #ff4c5b;
    color: #ff4c5b;
}

.container .discount,.container .specification {
    height: 88rpx;
    padding: 0 26rpx;
    display: flex;
    justify-content: space-between;
    line-height: 88rpx;
    box-sizing: border-box;
    background: #fff;
    margin-top: 20rpx;
}

.container .product-attrs .pro-attrs .attr-vals .attr-val.no-quantity {
    border: 1px dashed #ccc!important;
    color: #ccc!important;
}

.container .discount .disLeft .disIcon {
    vertical-align: middle;
    font-size: 40rpx;
    color: #ff7914;
    margin-right: 10rpx;
}

.container .discount .disLeft .title {
    vertical-align: middle;
    font-size: 26rpx;
    color: #666;
}

.container .discount .iconsize,.container .specification .iconsize {
    font-size: 28rpx;
    color: #999;
}

.container .particulars-line {
    height: 20rpx;
    background: #f4f4f4;
    width: 100%;
}

.container .speLeft {
    font-size: 26rpx;
    color: #333;
}

.container .speLeft .titChoose {
    margin-right: 30rpx;
}

.dm_module {
    padding: 0 26rpx;
}

.container .particulars-page .product-parameter {
