    height: 32rpx;
    border-radius: 50%;
    background-color: #ff4c5b;
    color: #fff;
    text-align: center;
    line-height: 32rpx;
    font-size: 22rpx;
}

.container .footnav .footLeft .shopCar .num2 {
    position: absolute;
    top: 10rpx;
    right: 26rpx;
    background-color: #ff4c5b;
    width: 16rpx;
    height: 16rpx;
    border-radius: 50%;
}

.container .footnav .footLeft .service contact-button {
    position: relative;
    opacity: 0;
    top: 2rpx;
}

.container .footnav .footLeft .service icon {
    position: relative;
    left: 20rpx;
}

.container .footnav .footLeft icon {
    font-size: 38rpx;
    width: 38rpx;
    height: 38rpx;
    line-height: 38rpx;
    margin: 4rpx 0;
}

.container .footnav .footLeft .favorite .collected {
    color: #ffc600;
}

.container .footnav .footLeft text {
    display: block;
    font-size: 20rpx;
}

.container .footnav .footMiddle,.container .footnav .footRight {
    width: 230rpx;
    line-height: 98rpx;
    text-align: center;
    font-size: 30rpx;
}

.container .footnav .footMiddle {
    background-color: rgba(255,114,0,0.2);
    color: #ff4c5b;
}

.container .footnav .footRight {
    background-color: #ff4c5b;
    color: #fff;
}

