page {
    background-color: #f4f4f4;
}

.container {
    display: flex;
    flex-direction: column;
    box-sizing: border-box;
}

.container .commission-info {
    height: 200rpx;
    background-color: #ff7200;
    text-align: center;
    width: 100%;
}

.container .commission-info .commission-money {
    padding-top: 50rpx;
    margin-bottom: 24rpx;
    font-size: 60rpx;
    line-height: 60rpx;
    width: 100%;
    color: #fff;
}

.container .commission-info .commission-msg {
    font-size: 28rpx;
    line-height: 28rpx;
    color: rgba(255,255,255,0.65);
    width: 100%;
    text-align: center;
}

.container .filtrate {
    height: 96rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    background-color: #fff;
    padding: 0 26rpx;
}

.container .filtrate .filtrate-name {
    display: flex;
    align-items: center;
}

.container .filtrate .record {
    font-size: 30rpx;
    color: #333;
}

.container .filtrate .commission-filtrate .filtrate-nav {
    font-size: 30rpx;
    color: #666;
}

.container .filtrate .commission-filtrate .filtrate-nav icon {
    margin-left: 10rpx;
    margin-top: -10rpx;
}

.container .filtrate-time {
    height: 96rpx;
    padding: 0 26rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    background-color: #fff;
}

.container .filtrate-time .filtrateName {
    font-size: 30rpx;
    color: #333;
}

.container .filtrate-time .filtrate-date {
    display: flex;
    font-size: 30rpx;
    line-height: 30rpx;
    color: #666;
    align-items: center;
}

.container .filtrate-time .filtrate-date .status {
    margin-left: 16rpx;
}

.container .timeBox {
    height: 88rpx;
    padding: 0 26rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 30rpx;
    color: #999;
}

.container .content-msg {
    background-color: #fff;
    padding: 0 26rpx;
    display: flex;
    flex-direction: column;
}

.commission,.order,.order-amount,.order-status,.commission-status,.Contributor-Member {
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 96rpx;
}

.commission-name,.order-name,.amount-name,.status-name,.member-name {
    font-size: 30rpx;
    color: #333;
}

.commission-num,.order-time,.amount,.order-status .status,.Contributor-Member .name {
    font-size: 30rpx;
    color: #666;
}

.order .order-time .time {
    margin-left: 16rpx;
}

.commission-status .status {
    font-size: 30rpx;
    color: #ff7200;
}

.container .content-line {
    height: 20rpx;
}

.container .content-msg:last-child {
    margin-bottom: 80rpx;
}