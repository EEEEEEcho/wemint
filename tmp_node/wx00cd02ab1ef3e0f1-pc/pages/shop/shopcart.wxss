.container {
}

.product-list {
    background-color: #fff;
}

.product-list.active {
    height: 130px;
    overflow: hidden;
}

.product-item {
    display: flex;
    width: 100%;
    align-items: center;
    box-sizing: border-box;
    height: 240rpx;
}

.image-item {
    list-style-type: none;
    display: block;
    overflow: hidden;
    max-width: 100%;
    width: 100%;
    position: relative;
}

.dummy {
    padding-top: 100%;
}

.product-item .col1 {
    flex: 0 0 40%;
    padding-right: 26rpx;
    box-sizing: border-box;
    height: 100%;
    display: flex;
    flex-direction: row;
    align-items: center;
    overflow: hidden;
}

.product-item .col1 .leftSelect {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 120rpx;
    height: 100%;
    position: relative;
}

.product-item .col1 .leftSelect .leftSelectBg {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 1;
}

.product-item .col1 .checkboxStyle {
    position: absolute;
    opacity: 0;
    left: 0;
    top: 50%;
    transform: translateY(-50%);
    z-index: 5;
}

.itemRight {
    flex: 1;
    height: 100%;
}

.product-item .col2,.product-item .showItem {
    position: relative;
    height: 100%;
    padding: 30rpx 26rpx 30rpx 0;
    box-sizing: border-box;
}

.product-item .showItem {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.product-item .image {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    max-width: 100%;
    max-height: 100%;
    display: block;
    margin: auto;
}

.product-item .showItem .price {
    position: absolute;
    bottom: 30rpx;
    left: 0;
    width: 100%;
    font-size: 28rpx;
    color: #333;
    font-weight: bold;
}

.product-item .name {
    width: 100%;
    font-size: 26rpx;
    color: #333;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.product-item .norms {
    position: relative;
    margin-top: 12rpx;
    padding-right: 50rpx;
    box-sizing: border-box;
    width: 100%;
    font-size: 22rpx;
    color: #999;
    height: 30rpx;
}

.itemNum {
    position: absolute;
    right: 0;
    top: 50%;
    transform: translateY(-50%);
}

.product-item .col2 {
    padding-right: 0;
    display: flex;
    align-items: center;
}

.product-item .numinfo {
    flex: 1;
    display: flex;
    flex-direction: row;
}

.product-item .del {
    display: flex;
    align-items: center;
    justify-content: center;
    flex: 0 0 120rpx;
    height: 176rpx;
    background-color: #ff7200;
    font-size: 28rpx;
    color: #fff;
}

.numinfo>view {
    display: inline-block;
}

.numinfo .num {
    width: 138rpx;
    height: 64rpx;
    text-align: center;
    font-size: 32rpx;
    line-height: 64rpx;
}

.numinfo .num-jian,.numinfo .num-jia {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 64rpx;
    height: 64rpx;
    background-color: #f4f4f4;
    font-size: 26rpx;
    color: #666;
}

.num-jian.active,.num-jia.active {
    background-color: #ff7200;
    color: #fff;
}

.suminfo {
    display: flex;
    clear: both;
}

.suminfo-desc {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    flex: 1;
    font-size: 28rpx;
    height: 98rpx;
    padding-left: 26rpx;
}

.headerOperate {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 96rpx;
    display: flex;
    align-items: center;
    justify-content: flex-end;
    background-color: #fff;
    z-index: 2;
}

.headerOperate text {
    font-size: 28rpx;
    display: inline-block;
    color: #333;
}

.headerOperate .ticket {
    color: #ff7200;
}

.headerOperate .changeStatus,.headerOperate .ticket {
    display: flex;
    width: 100rpx;
    align-items: center;
    justify-content: center;
    height: 100%;
}

.failureGoods {
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 96rpx;
    padding: 0 26rpx;
    color: #333;
    background-color: #fff;
}

.failureGoodsTitle {
    font-size: 26rpx;
}

.failureGoodsqk {
    font-size: 28rpx;
}

.product-item .col1 text {
    display: inline-block;
    font-size: 26rpx;
    color: #666;
}

.noEfficacy.product-list {
    opacity: .5;
}

.product-item .col1 .loseEfficacyText {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 120rpx;
    height: 100%;
}

.suminfo {
    position: fixed;
    display: flex;
    flex-direction: row;
    bottom: 0;
    left: 0;
    height: 98rpx;
    background-color: #fff;
    width: 100%;
    z-index: 9;
}

.suminfo::before {
    position: absolute;
    left: 0;
    top: 0px;
    width: 100%;
    border-top: 1px solid #ddd;
    content: '';
    transform-origin: 0 0;
}

@media only screen and (-webkit-min-device-pixel-ratio:2) {
    .suminfo::before {
        transform: scaleY(0.5);
    }
}

@media only screen and (-webkit-min-device-pixel-ratio:3) {
    .suminfo::before {
        transform: scaleY(0.33);
    }
}

.container .sumBottom {
    bottom: 0;
}

.suminfo-submit {
    flex: 0 0 192rpx;
    width: 192rpx;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    background: #ff7200;
    color: #fff;
    font-size: 30rpx;
}

.allCheckedBox {
    display: flex;
    position: relative;
    height: 100%;
    align-items: center;
    font-size: 26rpx;
    color: #999;
}

.allCheckedBox .allCheckItem {
    position: absolute;
    left: 0;
    top: 50%;
    transform: translateY(-50%);
    opacity: 0;
}

.allCheckedBox .iconText {
    margin-right: 16rpx;
}

.totalBox {
    color: #333;
    font-size: 26rpx;
    margin-right: 16rpx;
}

.total-money {
    color: #ff7200;
    font-size: 30rpx;
    font-weight: bold;
}

.favoriteBox {
    height: 98rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    flex: 0 0 192rpx;
    background-color: #ffe3cc;
    color: #ff7200;
    font-size: 30rpx;
}

.noproduct {
    display: flex;
    height: 200px;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    font-size: 32rpx;
    color: #666;
    padding-top: 20rpx;
}

.noproduct image {
    width: 50%;
}

.container .mark {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 10;
    background: rgba(0,0,0,0.7);
    transition: all 0.3s ease;
}

.container .hideParam {
    position: fixed;
    bottom: 0;
    transform: translateY(150%);
    transition: all 0.3s linear;
    z-index: 100;
    width: 100%;
}

.container .showParam {
    position: fixed;
    bottom: 0;
    transform: translateY(0);
    transition: all 0.3s linear;
    z-index: 100;
    width: 100%;
}

.container .discountList {
    height: 820rpx;
    background-color: #fff;
    width: 100%;
}

.container .discountList .dis-top {
    text-align: center;
    height: 96rpx;
    line-height: 96rpx;
    width: 100%;
    position: relative;
}

.container .discountList .dis-title {
    font-size: 30rpx;
    text-align: center;
    color: #666;
    width: 100%;
}

.container .discountList .markClose {
    position: absolute;
    width: 96rpx;
    height: 96rpx;
    top: 0;
    right: 0rpx;
    font-size: 28rpx;
    color: #666;
    display: flex;
    align-items: center;
    justify-content: center;
}

.container .discountList .coupon2 {
    padding: 0rpx 26rpx 96rpx;
    box-sizing: border-box;
    width: 100%;
}

.container .discountList .coupon2 .couponGetOver {
    width: 100%;
    text-align: center;
}

.container .discountList .coupon2 .couponGetOver image {
    margin-top: 220rpx;
    width: 200rpx;
}

.container .discountList .coupon2 .couponGetOver view {
    font-size: 22rpx;
    color: #999;
}

.container .discountList .coupon2 .coupon-item {
    padding: 26rpx 0;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.container .discountList .coupon2 .coupon-item:last-child {
    border-bottom: none;
}

.container .discountList .coupon2 .coupon-item .title {
    font-size: 34rpx;
    color: #ff7200;
    padding-bottom: 18rpx;
}

.container .discountList .coupon2 .coupon-item .title .titleText {
    margin-right: 50rpx;
}

.container .discountList .coupon2 .coupon-item .intro {
    font-size: 22rpx;
    color: #666;
    padding-bottom: 14rpx;
}

.container .discountList .coupon2 .coupon-item .coupon-time {
    font-size: 22rpx;
    color: #999;
}

.container .discountList .coupon2 .coupon-item .button .getbtn {
    width: 120rpx;
    height: 66rpx;
    line-height: 66rpx;
    text-align: center;
    border: 1px solid #ff7200;
    color: #ff7200;
    border-radius: 10rpx;
    font-size: 26rpx;
}

.container .discountList .coupon2 .coupon-item .button .hasGet {
    width: 120rpx;
    height: 66rpx;
    line-height: 66rpx;
    text-align: center;
    font-size: 26rpx;
    color: #999;
}

.hintPlug {
    position: fixed;
    top: 30%;
    left: 50%;
    transform: translate(-50%,-50%);
    padding: 50rpx;
    border-radius: 20rpx;
    background-color: rgba(0,0,0,.7);
    font-size: 26rpx;
    color: #fff;
    z-index: 110;
}