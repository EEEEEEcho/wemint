.row {
    display: flex;
    flex-direction: row;
}

.column {
    display: flex;
    flex-direction: column;
}

text {
    font-size: 28rpx;
    color: #51515c;
}

.lineStyle {
    height: 2rpx;
    background: #eeeeee;
    width: 100%;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}