@-webkit-keyframes open {
    from {
        transform: translateY(-200rpx);
    }

    to {
        transform: translateY(0rpx);
    }
}

@keyframes open {
    from {
        transform: translateY(-200rpx);
    }

    to {
        transform: translateY(0rpx);
    }
}

@-webkit-keyframes close {
    from {
        transform: translateY(0rpx);
    }

    to {
        transform: translateY(-900rpx);
    }
}

@keyframes close {
    from {
        transform: translateY(0rpx);
    }

    to {
        transform: translateY(-900rpx);
    }
}

.open {
    animation: open 0.5s ease-in-out both;
}

.close {
    animation: close 0.5s ease-in-out both;
}

@-webkit-keyframes showMask {
    from {
        opacity: 0;
    }

    to {
        opacity: 1;
    }
}

@keyframes showMask {
    from {
        opacity: 0;
    }

    to {
        opacity: 1;
    }
}

@-webkit-keyframes hideMask {
    from {
        opacity: 1;
    }

    to {
        opacity: 0;
    }
}

@keyframes hideMask {
    from {
        opacity: 1;
    }

    to {
        opacity: 0;
    }
}

.showMask {
    animation: showMask 0.5s ease-in-out both;
}

.hideMask {
    animation: hideMask 0.5s ease-in-out both;
}

