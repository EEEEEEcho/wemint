<navigate cartPage="{{cartPage}}"></navigate>
<view style="margin-top: {{navH+status+10}}px;">
    <view class="nickname">
        <view catchtap="getBlur" class="text_style" wx:if="{{!isShowInput}}">{{inputValue}}</view>
        <input bindblur="blurInput" bindinput="keepUpdate" class="name" focus="true" placeholderClass="placeholder" type="number" value="{{inputValue}}" wx:else></input>
    </view>
    <view class="tip" wx:if="{{isProfit}}">提示：分成比例只能是整数</view>
    <view catchtap="confirm" class="btn" disabled="{{isDisabled}}" style="{{isDisabled?'':'opacity:0.2'}}">保存</view>
</view>
