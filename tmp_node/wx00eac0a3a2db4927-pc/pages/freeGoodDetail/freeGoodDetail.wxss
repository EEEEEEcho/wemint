@-webkit-keyframes slideUp {
    from {
        transform: translateY(300rpx);
    }

    to {
        transform: translateY(0);
    }
}

@keyframes slideUp {
    from {
        transform: translateY(300rpx);
    }

    to {
        transform: translateY(0);
    }
}

@-webkit-keyframes slideDown {
    from {
        transform: translateY(0);
    }

    to {
        transform: translateY(300rpx);
    }
}

@keyframes slideDown {
    from {
        transform: translateY(0);
    }

    to {
        transform: translateY(300rpx);
    }
}

.slideUp {
    animation: slideUp 0.2s ease-in both;
}

.slideDown {
    animation: slideDown 0.2s ease-in both;
}

