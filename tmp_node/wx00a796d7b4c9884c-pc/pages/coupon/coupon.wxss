page {
    background: #f3f3f3;
}

.s_pagebg {
    width: 100%;
    float: left;
    padding: 0 0 20% 0;
}

.s_head {
    width: 100%;
    height: 60px;
    background: #fff;
}

.os_1 {
    width: 33.33%;
    height: 58px;
    float: left;
    margin: 0;
}

.os_1 view.navig {
    width: 100%;
    float: left;
}

.os_1 view.navig .font1 {
    width: 100%;
    float: left;
    height: 58px;
    font-size: 16px;
    color: #333;
    line-height: 58px;
    text-align: center;
    font-weight: normal;
    padding: 0;
    border-bottom: 2px solid #fff;
}

.os_1 view.navig .font2 {
    width: 100%;
    float: left;
    height: 58px;
    font-size: 16px;
    color: #333;
    line-height: 58px;
    text-align: center;
    font-weight: normal;
    padding: 0;
    border-bottom: 2px solid #9139e1;
}

.s_box {
    width: 100%;
    height: auto;
    float: left;
    padding: 10px 0;
    margin: 1% 0 8% 0;
}

.yhuiq {
    width: 94%;
    float: left;
    position: relative;
    z-index: 20;
    margin: 3% 0 1% 3%;
}

.yhtu {
    width: 100%;
    height: auto;
    float: left;
    position: relative;
    z-index: 5;
}

.yhtu image {
    width: 100%;
}

.yh_con {
    width: 100%;
    float: left;
    position: absolute;
    z-index: 10;
    margin: 0%;
    left: 0;
    top: 0;
}

.yh_con .h3 {
    width: 28%;
    height: 80px;
    float: left;
    margin: 6% 5% 2% 1%;
    font-family: Arial;
    font-size: 26px;
    color: #fff;
    line-height: 40px;
    text-align: center;
    font-weight: normal;
}

.yh_con .h3 .font1 {
    width: 100%;
    height: 30px;
    float: left;
    margin: 0% 0% 0% 0%;
    font-family: 'Microsoft YaHei';
    font-size: 12px;
    color: #fff;
    line-height: 30px;
    text-align: center;
    font-weight: normal;
    overflow: hidden;
}

.yh_con .h4 {
    width: 50%;
    height: 80px;
    float: left;
    margin: 3% 1% 2% 2%;
    font-family: 'Microsoft YaHei';
    font-size: 20px;
    color: #333;
    line-height: 40px;
    text-align: left;
    font-weight: normal;
}

.yh_con .h4 .font1 {
    width: 100%;
    height: auto;
    float: left;
    margin: 0% 0% 0% 0%;
    font-family: 'Microsoft YaHei';
    font-size: 13px;
    color: #999999;
    line-height: 20px;
    text-align: left;
    font-weight: normal;
    overflow: hidden;
}

#foot {
    width: 100%;
    float: left;
    padding: 5px 0;
    background: #fff;
    position: fixed;
    z-index: 99;
    bottom: 0;
    box-shadow: 0px -10px 15px -10px #dcdcdc;
    overflow: hidden;
}

#foot .navig {
    width: 25%;
    float: left;
    margin: 0 0 0 0%;
}

#foot .navig .fot1 {
    width: 100%;
    float: left;
}

#foot .navig .fot1 image.img1 {
    width: 25%;
    display: block;
    margin: 0 auto;
    border: none;
}

#foot .navig .fot1 image.img2 {
    width: 25%;
    display: none;
    margin: 0 auto;
    border: none;
}

#foot .navig .fot1 .font {
    width: 100%;
    height: 30px;
    float: left;
    margin: 0;
    font-family: ' Microsoft YaHei';
    font-weight: normal;
    font-size: 14px;
    line-height: 30px;
    color: #333;
    text-align: center;
    overflow: hidden;
}

#foot .navig .fot2 {
    width: 100%;
    float: left;
}

#foot .navig .fot2 image.img1 {
    width: 25%;
    display: none;
    margin: 0 auto;
    border: none;
}

#foot .navig .fot2 image.img2 {
    width: 25%;
    display: block;
    margin: 0 auto;
    border: none;
}

#foot .navig .fot2 .font {
    width: 100%;
    height: 30px;
    float: left;
    margin: 0;
    font-family: ' Microsoft YaHei';
    font-weight: normal;
    font-size: 14px;
    line-height: 30px;
    color: #c586ff;
    text-align: center;
    overflow: hidden;
}