.container {
    position: relative;
    width: 100%;
    height: 1220rpx;
    background-color: #fff;
    color: #939393;
}

.nav_left {
    display: inline-block;
    width: 100%;
    height: 100%;
    background: #f3f3f3;
    text-align: center;
    left: 0;
    top: 0;
}

.nav_left .nav_left_items {
    border-left: 2px solid #f3f3f3;
    height: 80rpx;
    line-height: 80rpx;
    padding: 15rpx 0;
    background: #f3f3f3 url(data:image/jpeg;base64,) no-repeat center bottom;
    background-size: 90%;
    font-size: 29rpx;
    color: #666666;
    font-weight: normal;
}

.nav_left .nav_left_items.active {
    background: #fff;
    color: #333;
    border-left: 2px solid #c586ff;
}

.scroll_right {
    position: fixed;
    top: 0;
    right: 0;
    overflow: auto;
    flex: 1;
    width: 75%;
    height: 100%;
    padding: 20rpx;
    box-sizing: border-box;
    background-color: #fff;
}

.mink::after {
    display: block;
    content: '';
    clear: both;
}

.jiul,.jiul image {
    width: 100%;
    height: 170rpx;
}

.minl {
    font-size: 29rpx;
    color: #777;
    text-align: left;
    line-height: 60rpx;
    float: left;
    width: 100%;
}

.mink {
    width: 100%;
    background: #fff;
    height: 100%;
}

.mink .h3 {
    width: 100%;
    height: 40px;
    float: left;
    font-family: ' Microsoft YaHei';
    font-size: 15px;
    color: #333333;
    line-height: 20px;
    text-align: left;
    font-weight: normal;
    text-transform: uppercase;
    overflow: hidden;
}

.nav_right_items {
    float: left;
    width: 33.33%;
    text-align: center;
    color: #4a4a4a;
    background: #fff;
}

.nav_right_items image {
    width: 60px;
    height: 50px;
    margin-top: 15rpx;
}

.nav_right_items text {
    display: block;
    margin-top: 5rpx;
    margin-bottom: 10rpx;
    font-size: 26rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.other-navigator-hover {
    background: #fff;
}

.scroll_left {
    width: 25%;
    height: 100%;
    background: #f3f3f3;
    text-align: center;
    position: fixed;
    left: 0;
    top: 0;
}