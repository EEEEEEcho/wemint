.s_list_bg {
    width: 100%;
    float: left;
    padding: 5px 0;
    border-bottom: 1px solid #d6d6d6;
}

.s_list_1 {
    width: 50%;
    float: left;
    text-align: center;
    display: flex;
    justify-content: center;
}

.s_list_1 view.navig .font1 {
    display: inline-block;
    margin: 0 auto;
    font-family: ' Microsoft YaHei';
    font-weight: normal;
    font-size: 16px;
    line-height: 30px;
    color: #222222;
    text-align: center;
}

.s_list_1 view.navig {
    margin: 0 auto;
}

.s_list_1 view.navig image {
    width: 14px;
    height: 25px;
    float: right;
    margin: 0px 0 0 0%;
}

.m_box1 {
    width: 100%;
    float: left;
    padding: 0px 0;
    margin: 2% 0 6% 0;
}

.m_box1_2 {
    width: 45%;
    float: left;
    margin: 2% 1% 1% 3%;
}

.m_box1_2 navigator.navig {
    width: 100%;
    float: left;
}

.m_box1_2 navigator.navig image {
    width: 100%;
    float: left;
    border-radius: 20px;
}

.m_box1_2 navigator.navig .h3 {
    width: 100%;
    height: 40px;
    float: left;
    font-family: ' Microsoft YaHei';
    font-size: 15px;
    color: #333333;
    line-height: 20px;
    text-align: left;
    font-weight: normal;
    text-transform: uppercase;
    overflow: hidden;
}

.m_box1_2 navigator.navig .h4 {
    width: 100%;
    height: 30px;
    float: left;
    font-family: ' Microsoft YaHei';
    font-size: 13px;
    color: #ff0000;
    line-height: 30px;
    text-align: left;
    font-weight: normal;
    text-transform: uppercase;
    overflow: hidden;
}

.m_box1_2 navigator.navig .h4 .font1 {
    float: right;
    font-family: ' Microsoft YaHei';
    font-size: 13px;
    color: #acacac;
    line-height: 30px;
    text-align: left;
    font-weight: normal;
    text-transform: uppercase;
}