.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
}

button-hover {
    background: none;
}

.dots {
    width: 100%;
    float: left;
    text-align: center;
}

.dot {
    display: inline-block;
    background: #c9c9c9;
    text-indent: 9999px;
    width: 45px;
    height: 6px;
    margin: 14rpx 10rpx;
    border-radius: 10px;
    overflow: hidden;
}

.active {
    background: #c586ff;
}

button::after {
    border: none;
}

button::focus {
    border: none;
}

.max1 {
    width: 100%;
    float: left;
    margin: 2% 0 3% 0;
    position: relative;
    z-index: 1;
    border: none;
}

.max1 .search {
    width: 93%;
    float: left;
    height: 36px;
    border-radius: 20px;
    background: #f3f3f3;
    border: 1px solid #f3f3f3;
    font-size: 14px;
    line-height: 36px;
    color: #c586ff;
    overflow: hidden;
    margin-left: 3%;
    margin-bottom: 3%;
}

.max1 .search input {
    height: 36px;
    line-height: 36px;
    width: 70%;
    float: left;
    margin-left: 10px;
    border: none;
    color: #c586ff;
}

.max1 .search button.cjjz {
    background: transparent;
    border: 0px;
    width: 23px;
    float: LEFT;
    padding: 0rpx;
    position: inherit;
    margin: 2% 2% 0 4%;
}

.max1 .search button.cjjz::after {
    border: none;
}

.max1 .search image {
    width: 23px;
    height: 23px;
    float: left;
}

