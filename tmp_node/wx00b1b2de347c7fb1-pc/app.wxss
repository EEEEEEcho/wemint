page {
    position: absolute;
    background-color: #ecf0f1;
    top: 0px;
    left: 0px;
    right: 0px;
    bottom: 0px;
}

.container {
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
    font-size: 32rpx;
    overflow: hidden;
}

.bg-c-a {
    background-color: #ecf0f1;
}

.bg-c-e {
    background-color: #eee;
}

.bg-c-f {
    background-color: #fff;
}

.clearfix:after {
    content: "";
    height: 0;
    line-height: 0;
    display: block;
    visibility: hidden;
    clear: both;
}

.pull-left {
    float: left;
}

.pull-right {
    float: right;
}

.dis-block {
    display: block;
}

.dis-inblock {
    display: inline-block;
}

.w-100p {
    width: 100%;
}

.txt-ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.txt-3-ellipsis {
    word-wrap: break-word;
    word-break: normal;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 3;
}

.padder-l {
    padding: 25rpx;
}

.padder-s {
    padding: 10rpx;
}

.padder-ss {
    padding: 5rpx;
}

.padder-t-m {
    padding-top: 20rpx;
}

