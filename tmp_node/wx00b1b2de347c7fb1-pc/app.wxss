}

.m-l-s {
    margin-left: 10rpx;
}

.m-r-m {
    margin-right: 20rpx;
}

.m-b-ss {
    margin-bottom: 5rpx;
}

.m-r-s {
    margin-right: 10rpx;
}

.m-r-ss {
    margin-right: 5rpx;
}

.m-m {
    margin: 20rpx;
}

.m-s {
    margin: 10rpx;
}

.flex-box {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}

.no-border {
    border: none!important;
}

.border-b {
    border-bottom: 1rpx solid #ecf0f1;
}

.deal-list {
    margin: 8rpx 0;
    width: 100%;
    background-color: #fff;
}

.deal-tab {
    border-bottom: 8rpx solid #ecf0f1;
}

.deal-item {
    padding: 20rpx 10rpx;
    border-bottom: 8rpx solid #ecf0f1;
}

.deal-item .deal-header .head {
    overflow: hidden;
    margin-right: 20rpx;
}

.deal-item .deal-header .head image {
    width: 120rpx;
    height: 120rpx;
    border-radius: 5px;
}

.deal-item .deal-header .name {
    width: 80%;
    color: skyblue;
}

.deal-item .deal-header .name .call,.delete {
    background-color: #e60012;
    border-radius: 5px;
    padding: 8rpx 20rpx;
    color: #fff;
}

.deal-item .deal-header .name .user-name {
    margin-bottom: 10rpx;
    width: 300rpx;
    color: #589cc7;
}

.deal-item .deal-header .name .keyword {
    color: #f4a58d;
    border: 1px solid #f4a58d;
    padding: 2px 5px;
    margin-right: 5px;
}

.deal-item .top {
    color: #fff;
    border: 1px solid #e60012;
    background-color: #e60012;
    padding: 2px 5px;
    margin-right: 5px;
}

.deal-item .deal-content .good-image {
    flex: 0 1 auto;
    width: 30%;
    height: 240rpx;
    background-color: #fff;
    text-align: center;
    margin: 10rpx;
}

.deal-item .discuss {
    width: 100%;
    line-height: 1;
}

.discuss text {
    vertical-align: middle;
    color: #e60012;
}

.link-txt {
    color: #1171c1;
}

.txt-c {
    text-align: center;
}

.strong {
    font-weight: bolder;
    margin: 10rpx 0;
    text-indent: 2em;
}