.bt {
    width: 70%;
    display: flex;
    flex-direction: column;
    float: left;
}

.btMyIcon {
    width: 30%;
    display: flex;
    flex-direction: column;
    float: right;
}

.userinfo {
    display: flex;
    flex-direction: column;
    align-items: center;
    float: right;
}

.userinfo-avatar {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
}

.userinfo-nickname {
    color: #4F4F4F;
    font-size: 12px;
}

.myCurr-view {
    display: flex;
    width: 700rpx;
    padding-top: 50rpx;
    margin-left: 25rpx;
    min-height: 250rpx;
}

.curr {
    width: 100%;
    box-shadow: 0 0 10px 0 #ADADAD;
    border-radius: 10rpx;
    padding-bottom: 20rpx;
    background-color: #FFFFFF;
}

.currTitle {
    display: flex;
    justify-content: center;
    height: 60rpx;
    font-size: 40rpx;
    margin-top: 25rpx;
    color: #9D9D9D;
}

.currContent {
    display: flex;
    height: 100rpx;
    padding-top: 10rpx;
}

.currContent:active {
    background: #eee;
}

.currContent_time {
    margin-top: 15rpx;
    width: 15rpx;
    height: 90rpx;
}

.currContent_list1 {
    width: 25%;
    display: flex;
    justify-content: center;
    font-size: 30rpx;
}

.currContent_list2 {
    display: flex;
    justify-content: center;
    width: 5%;
}

.currContent_list3 {
    width: 40%;
    padding-left: 10rpx;
}

.lable {
    font-size: 30rpx;
    color: #3C3C3C;
    font-weight: 600;
}

.content {
    font-size: 25rpx;
    color: #9D9D9D;
}

.currContent_list4 {
    width: 25%;
    font-size: 30rpx;
    color: #9D9D9D;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.currNo {
    display: flex;
    justify-content: center;
    height: 60rpx;
    font-size: 40rpx;
    margin-top: 50rpx;
    color: #9D9D9D;
}

.currNoIcon {
    width: 50rpx;
    height: 50rpx;
}

.classCurr_view_but {
    font-size: 10px;
    color: #f8ac59;
    background-color: #fff;
    border: 1px solid #f8ac59;
    margin-top: 0px;
    width: 170rpx;
    border-radius: 25px;
}

.classCurr_view_but_hover {
    font-size: 10px;
    color: #fff;
    background-color: #f8ac59;
    border: 1px solid #fff;
    margin-top: 0px;
    width: 170rpx;
    border-radius: 25px;
}

.client_head_back_style {
    margin-top: 30rpx;
}

.client_head_style {
    float: left;
    background: white;
    width: 25%;
    height: 200rpx;
}

.client_head_style:active {
    background: #eeeeee;
}

.client_head_style image {
    display: block;
    height: 120rpx;
    width: 120rpx;
    border-radius: 80rpx;
    margin: 20rpx auto;
    background: white;
}

.client_head_style text {
    display: block;
    font-size: 34rpx;
    color: #222c38;
    text-align: center;
    margin: 10rpx auto;
}

.client_style_yfa_content image {
    width: 350rpx;
    height: 175rpx;
    float: left;
    margin-left: 10rpx;
    margin-top: 10rpx;
}

.tongzhi {
    display: flex;
    font-size: 20px;
    font-weight: 700;
    margin-left: 25rpx;
}

.line {
    font-size: 30rpx;
    padding-right: 0rpx;
    line-height: 3;
    border-bottom: 1px solid #efefef;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #222c38;
}

.line1 {
    font-size: 30rpx;
    padding-right: 0rpx;
    line-height: 3;
    border-bottom: 1px solid #efefef;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.line:active {
    background: #eee;
}

.line1:active {
    background: #eee;
}

.lineImage {
    width: 15rpx;
    height: 15rpx;
    margin-bottom: 5rpx;
}

.section .line:before {
    content: '>';
    float: right;
    color: #aaa;
    margin-left: 20rpx;
}