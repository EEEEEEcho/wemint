.cart_container {
    display: flex;
    flex-direction: row;
    border-bottom: 0px solid #efefef;
}

.column {
    display: flex;
    flex-direction: column;
    top: 15rpx;
    width: 180rpx;
}

.column_title {
    font-size: 30rpx;
    margin-top: 30rpx;
}

.column_stepper {
    margin-top: 20rpx;
}

.column_bir {
    width: 100rpx;
    margin-top: 30rpx;
    font-size: 30rpx;
    color: #6C6C6C;
}

.column_ccm {
    width: 300rpx;
    margin-top: 30rpx;
    font-size: 30rpx;
    color: #6C6C6C;
}

.item-image {
    width: 80rpx;
    height: 80rpx;
    margin: 10rpx;
    border-radius: 50%;
}

.classBag {
    display: flex;
    flex-direction: row;
}

.classBag .htl {
    width: 33.3%;
    border: 1px solid #d0d0d0;
    text-align: center;
    color: #FF8247;
}

.classBag .text {
    width: 33.3%;
    border: 1px solid #d0d0d0;
    text-align: center;
    color: #FF8247;
}