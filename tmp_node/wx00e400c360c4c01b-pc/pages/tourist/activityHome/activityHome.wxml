</view>
<view class="userinfobt">
    <view bindtap="gengduo" class="gengduo" data-is_pay="1">更多</view>
</view>
<view class="section">
    <view class="wnr" wx:if="{{activityList.length==0}}">
        <image src="http://p7mq9gjza.bkt.clouddn.com/non.png"></image>
    </view>
    <view class="acList" wx:if="{{activityList.length>0}}">
        <view bindtap="viewTrip" class="m-trip">
            <image class="cover" src="{{ac1.img_path}}"></image>
        </view>
        <view class="acname">{{ac1.title}}</view>
        <view class="acbt">电话咨询 {{mobile}}</view>
        <view class="acBtn">
            <form reportSubmit bindsubmit="view">
                <button class="classCurr_view_but" data-id="{{ac1.id}}" data-model="{{ac1}}" formType="submit" hoverClass="classCurr_view_but_hover">预约体验 </button>
            </form>
        </view>
    </view>
</view>
