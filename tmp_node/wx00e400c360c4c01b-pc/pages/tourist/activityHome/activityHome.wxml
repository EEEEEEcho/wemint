<swiper autoplay="true" class="swiper" duration="1000" indicatorDots="true" interval="5000">
    <swiper-item wx:for="{{activityList}}">
        <image bindtap="topImgView" class="slide-image" data-id="{{item.id}}" src="{{item.img_path}}"></image>
    </swiper-item>
</swiper>
