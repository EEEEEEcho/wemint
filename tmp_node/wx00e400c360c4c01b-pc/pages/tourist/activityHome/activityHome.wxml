        <form reportSubmit bindsubmit="acList">
            <button class="formBtn" data-is_pay="0" formType="submit">
                <image src="{{ac_menu_02}}"></image>
            </button>
        </form>
        <text>云课堂</text>
    </view>
    <view class="client_head_style" id="6">
        <form reportSubmit bindsubmit="acUe">
