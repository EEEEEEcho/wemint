.searchk {
    display: flex;
    padding: 30rpx;
}

.searchl {
    border: 1px solid #dddddd;
    border-radius: 6px;
    width: 565rpx;
    height: 60rpx;
    line-height: 60rpx;
    padding-left: 35rpx;
    background: #f7f8fc;
    display: flex;
    font-size: 14px;
}

.searchl icon {
    margin-top: 8rpx;
}

.searchl input {
    width: 460rpx;
    height: 60rpx;
    line-height: 60rpx;
    margin-left: 20rpx;
}

.searchr {
    width: 130rpx;
    text-align: center;
    line-height: 60rpx;
}

.coupontitle {
    border-bottom: 1px solid #dcdcdc;
    height: 100rpx;
    line-height: 100rpx;
    border-top: 1px solid #dcdcdc;
    background: #fff;
}

.coupontitlel {
    width: 175rpx;
    text-align: center;
    font-size: 14px;
    float: left;
}

.coupontitlel view {
    border-bottom: 2px solid #fff;
    width: 145rpx;
    margin: 0 auto;
    height: 96rpx;
    position: relative;
}

.jiages1s {
    width: 225rpx;
}

.coupontitlel view.jiages1s {
    width: 170rpx;
}

.coupontitlel view.active {
    color: #fd5318;
}

.jiages::after {
    border: 6px solid transparent;
    border-top: 6px solid #b7b7b7;
    width: 0;
    height: 0;
    position: absolute;
    right: 3px;
    top: 55rpx;
    content: ' ';
}

.jiages::before {
    border: 6px solid transparent;
    border-bottom: 6px solid #b7b7b7;
    width: 0;
    height: 0;
    position: absolute;
    content: ' ';
    right: 3px;
    top: 25rpx;
}

.jiangxu::after {
    border: 6px solid transparent;
    border-top: 6px solid #fd5318;
    width: 0;
    height: 0;
    position: absolute;
    right: 3px;
    top: 55rpx;
    content: ' ';
}

.shengxu::before {
    border: 6px solid transparent;
    border-bottom: 6px solid #fd5318;
    width: 0;
    height: 0;
    position: absolute;
    content: ' ';
    right: 3px;
    top: 25rpx;
}

.kong {
    display: flex;
    flex-direction: column;
    align-items: center;
    background: #fff;
    padding: 210rpx 0;
    clear: both;
}

.kong image {
    width: 171rpx;
    height: 171rpx;
    margin-bottom: 50rpx;
}

.name {
    height: 84rpx;
    line-height: 42rpx;
    overflow: hidden;
    font-size: 14px;
    color: #2f3638;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.saleprice {
    font-size: 14px;
    color: #fc555c;
    position: relative;
    margin-top: 25rpx;
    font-family: Microsoft yahei;
}

.saleprice text {
    font-size: 20px;
}

.xialiang {
    position: absolute;
    top: 0px;
    right: 0px;
    color: #bcbcbc;
    font-size: 12px;
    line-height: 60rpx;
}

.goodcont {
    margin-top: 20rpx;
}

.goodlis {
    padding: 15rpx;
    margin-top: 30rpx;
    width: 330rpx;
    float: left;
    margin-left: 15rpx;
}

.goodlis image {
    width: 310rpx;
    height: 310rpx;
}

.goodlis:nth-child(2n) {
    margin-left: 10rpx;
}

.goodlis .name {
    margin-top: 15rpx;
}