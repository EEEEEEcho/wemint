.wllist {
    width: 700rpx;
    padding-left: 50rpx;
    position: relative;
    min-height: 180rpx;
}

.wllist::before {
    content: "";
    position: absolute;
    border: 3px solid #dedede;
    width: 20rpx;
    height: 20rpx;
    border-radius: 50%;
    display: block;
    left: 0rpx;
}

.wllist::after {
    content: "";
    position: absolute;
    border-left: 1px solid #dedede;
    width: 20rpx;
    height: 130rpx;
    display: block;
    left: 18rpx;
    top: 40rpx;
}

.active::before {
    border: 3px solid #f68d60;
}

.active {
    color: #f68d60;
}

.wllist view {
    color: #a8a8a8;
}