.hdonglist:last-child {
    border: none;
}

.hdonglist::after,.hdonglist::before {
    border: 10px solid transparent;
    border-left: 10px solid #fff;
    width: 0;
    position: absolute;
    right: 10px;
    height: 0;
    top: 90rpx;
    content: ' ';
}

.hdonglist::before {
    border-left-color: #bbbbbb;
    right: 8px;
}

