.container {
    display: flex;
    flex-direction: column;
    width: 100%;
    padding: 60rpx 0;
}

.userinfoimage {
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 100%;
    padding-bottom: 55rpx;
}

.userinfo-image {
    width: 174rpx;
    height: 109rpx;
    margin: 20rpx;
}

.loginpassword {
    display: flex;
    width: 100%;
    height: 98rpx;
    line-height: 98rpx;
}

.borderbott0m {
    border-bottom: 1px solid #dcdcdc;
}

.loginpassword input {
    height: 98rpx;
    line-height: 98rpx;
    color: #000;
    font-size: 14px;
    padding-left: 5rpx;
    width: 450rpx;
}

.loginpassword input:-moz-placeholder {
    color: #cecece;
}

.loginpassword text {
    font-size: 16px;
    padding-left: 35rpx;
}

