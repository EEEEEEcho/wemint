.footer {
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
    width: 100%;
    font-size: 24rpx;
    overflow: hidden;
    background: #fff;
    border-top: 1px solid #ababab;
    color: #8f8f8f;
    padding-bottom: 8rpx;
}

.footer navigator {
    width: 25%;
    text-align: center;
    float: left;
}

.footer navigator image {
    width: 48rpx;
    height: 48rpx;
    margin-top: 15rpx;
    display: inline-block;
    margin-bottom: 5rpx;
}