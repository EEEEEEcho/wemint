.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
}

page {
    font-family: microsoft yahei;
}

.bg-white {
    background: #fff;
}

.icon {
    display: flex;
    align-items: center;
    flex-direction: column;
}

image {
    display: flex;
}

