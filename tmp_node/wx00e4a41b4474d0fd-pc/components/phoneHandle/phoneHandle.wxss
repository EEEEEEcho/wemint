.mask2 {
    position: fixed;
    top: 0px;
    left: 0px;
    width: 100vw;
    height: 100vh;
    background-color: rgba(0,0,0,0.6);
    z-index: 9999;
}

.model {
    position: absolute;
    width: 75vw;
    left: 12.5vw;
    bottom: 40%;
    overflow: hidden;
    border-radius: 10rpx;
    background-color: #fff;
    z-index: 10000;
}

.model .modelTitle {
    margin-top: 30rpx;
    width: 100%;
    font-size: 40rpx;
    color: #000;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

.logo {
    border-radius: 50%;
    margin-left: 36rpx;
    width: 40rpx;
    height: 40rpx;
}

.yeslogo {
    border-radius: 50%;
    margin-left: 50rpx;
    width: 24rpx;
    height: 24rpx;
    margin-right: 20rpx;
}

.title {
    margin-left: 10rpx;
    font-size: 36rpx;
    font-weight: 700;
}

.modelBodyTitle {
    color: #000;
    box-sizing: border-box;
    width: 100%;
    font-weight: 500;
    padding: 20rpx 20rpx 20rpx 20rpx;
    font-size: 28rpx;
}

.model .modelBody {
    color: #000;
    margin-top: 20rpx;
    box-sizing: border-box;
    width: 100%;
    padding: 0rpx 20rpx 50rpx 20rpx;
    font-size: 28rpx;
}

.model .btns {
    width: 100%;
    text-align: center;
    display: flex;
    justify-content: space-around;
}

.model .btns .cancel {
    background: #EDEDED;
    width: 40vw;
    color: #000;
    border-radius: 0px;
    border-width: 0px;
}

.model .btns .agree {
    width: 40vw;
    background: #EDEDED;
    border-width: 0px;
    border-radius: 0px;
    color: #1AAD17;
}