.nav-wrap {
    position: fixed;
    width: 100%;
    top: 0;
    z-index: 98;
}

.nav-bottom {
    position: fixed;
    width: 100%;
    height: 5px;
    color: #000;
    z-index: 98;
}

.nav-title {
    position: absolute;
    text-align: center;
    max-width: 400rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    margin: auto;
    color: #fff;
    font-size: 32rpx;
    font-weight: bold;
}

.nav-capsule {
    position: absolute;
    display: flex;
    align-items: center;
    width: 180rpx;
    justify-content: space-between;
    height: 100%;
}

.nav-buildStyle {
    display: flex;
    align-items: center;
    justify-content: space-between;
    height: 100%;
}

.nav-buildCityStyle {
    display: flex;
    align-items: center;
    justify-content: space-between;
    height: 100%;
}

.back-pre {
    width: 24rpx;
    height: 24rpx;
    border-top: 4rpx solid #fff;
    border-left: 4rpx solid #fff;
    transform: rotate(-45deg);
    margin-left: 30rpx;
}

.back-pre-image {
    width: 30rpx;
    height: 30rpx;
    margin-left: 42rpx;
}

.nav-padding {
    width: 100%;
    background-color: #272A34;
}

.icon_xiala {
    width: 16rpx;
    height: 10rpx;
    position: absolute;
    right: 0px;
    top: 50%;
    transform: translateY(-50%);
}