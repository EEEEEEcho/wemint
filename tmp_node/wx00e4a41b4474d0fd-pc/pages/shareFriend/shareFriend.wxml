                                            <text style="color: #B07B56;" wx:if="{{item.state==6}}">已结佣</text>
                                        </view>
                                    </view>
                                    <view class="recommendList-status-content" wx:if="{{item.copyWriting}}">
                                        <text>{{item.copyWriting}}</text>
                                    </view>
                                </view>
                            </view>
                        </view>
                    </view>
                </view>
            </view>
            <view style="position:relative;width: 750rpx;height: 26rpx;"></view>
        </view>
    </view>
</scroll-view>
