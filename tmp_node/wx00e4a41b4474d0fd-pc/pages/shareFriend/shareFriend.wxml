                    <view class="wg-head-box" style="justify-content:center;margin-top:10vh;">
                        <image class="authHead" src="../../image/wepy_pro/default-head.png"></image>
                        <image class="authHead" src="../../image/wepy_pro/default-head.png"></image>
                        <image class="authHead" src="../../image/wepy_pro/default-head.png"></image>
                        <image class="authHead" src="https://dm.static.elab-plus.com/wepy_pro/shareFriend/more.png" style="border:none"></image>
                    </view>
                    <view style="margin-top:32rpx;text-align:center;color:#666666;font-size:26rpx">
                    *点击解锁查看成功推荐明细*
                </view>
                    <button bindgetphonenumber="getPhoneNumber" class="authPhoneBtn" openType="getPhoneNumber">立即解锁</button>
                </view>
            </view>
            <view class="recommend-list-noPhone" wx:elif="{{showPhoneModel&&noVerifiedRecommendList.length>0&&!specialUser}}">
