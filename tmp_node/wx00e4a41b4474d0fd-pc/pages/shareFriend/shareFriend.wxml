                                            </view>
                                        </view>
                                        <view class="recommendList-status-content" wx:if="{{item.copyWriting}}">
                                            <text>{{item.copyWriting}}</text>
                                        </view>
                                    </view>
                                </view>
                                <view class="recommend-list-noPhone" style="left: -50rpx;" wx:if="{{specialUser&&recommendList.length==0}}">
