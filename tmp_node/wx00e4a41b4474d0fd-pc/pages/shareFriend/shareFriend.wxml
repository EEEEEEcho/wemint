<myLoading id="myLoading"></myLoading>
<navbar data="{{navbar}}"></navbar>
<scroll-view scrollY="true" style="min-height:100vh">
    <view style="min-height:100vh">
        <view class="userInformation-container">
            <image class="userInformation-container-bg" mode="scaleToFill" src="https://dm.static.elab-plus.com/miniprogram/sharefriend/background.png"></image>
            <view class="userInformation-container-content">
                <view class="userInformation-container-top"></view>
                <view wx:if="{{!specialUser}}">
                    <view class="userInformation-container-Common">
                        <view class="userInformation-container-totalNum">
                            <text>{{recommendSuccessCount>9999?'1W+':recommendSuccessCount}}</text>
                        </view>
                        <view class="userInformation-container-description">
                            <text style="font-size: 34rpx;color: #FFFFFF;text-align: center;line-height: 36px;">成功推荐注册</text>
                        </view>
                    </view>
                </view>
                <view wx:else>
                    <view class="userInformation-container-totalNum">
                        <text>{{recommendSuccessCountWithBuild+recordCount>9999?'1W+':recommendSuccessCountWithBuild+recordCount}}</text>
                    </view>
                    <view class="userInformation-container-description">
                        <text wx:if="{{specialUserType==2||specialUserType==3}}">{{specialRoleName}}{{agentStatus=='-1'?'(已失效)':''}}{{agentName}}的客户</text>
                        <text style="font-size: 34rpx;color: #FFFFFF;text-align: center;line-height: 36px;" wx:else>成功推荐注册</text>
                    </view>
                    <view class="userInformation-container-recommend-container">
                        <view class="userInformation-container-recommend-body">
                            <view class="userInformation-container-recommend-success">
                                <text>{{recommendSuccessCountWithBuild>9999?'1W+':recommendSuccessCountWithBuild}}</text>
                            </view>
                            <view class="userInformation-container-recommend-success-title">
                                <text>成功推荐</text>
                            </view>
                            <view class="userInformation-container-recommend-divide"></view>
                            <view class="userInformation-container-recommend-liebian">
                                <text>{{recordCount>9999?'1W+':recordCount}}</text>
                            </view>
                            <view class="userInformation-container-recommend-liebian-title">
                                <text>分享裂变</text>
                            </view>
                        </view>
                    </view>
                </view>
                <view class="userInformation-container-recommend-bottom" wx:if="{{specialUserType}}">
                    <image class="userInformation-container-recommend-bottom-bg" mode="scaleToFill" src="https://dm.static.elab-plus.com/miniprogram/sharefriend/buildingBg.png"></image>
                    <view class="userInformation-container-recommend-bottom-body">
                        <view class="userInformation-container-recommend-bottom-body-left">
                            <view class="userInformation-container-recommend-bottom-body-left-image-container">
                                <image class="userInformation-container-recommend-bottom-body-left-image" mode="scaleToFill" src="https://dm.static.elab-plus.com/pankebao/buildIcon.png"></image>
                            </view>
                            <view class="userInformation-container-recommend-bottom-body-left-description">
                                <text wx:if="{{specialUserType==1}}">身份认证</text>
                                <text wx:if="{{specialUserType==2||specialUserType==3}}">{{specialRoleName}}</text>
                            </view>
                        </view>
                        <view class="userInformation-container-recommend-bottom-body-right">
                            <view bindtap="navigateFuc" class="userInformation-container-recommend-bottom-body-right-description">
                                <text wx:if="{{specialUserType==1}}">去认证</text>
                                <text wx:if="{{specialUserType==2}}">身份绑定</text>
                                <text wx:if="{{specialUserType==3}}">可结佣状态</text>
                            </view>
                        </view>
                    </view>
                </view>
            </view>
            <view bindtap="navigateToOrganize" class="userInformation-container-organize" wx:if="{{specialUser}}">
                <image class="userInformation-container-organize-bg" mode="scaleToFill" src="https://dm.static.elab-plus.com/miniprogram/sharefriend/originazre_bg.png"></image>
                <view class="userInformation-container-organize-content">
                    <image class="userInformation-container-organize-content-logo" mode="scaleToFill" src="https://dm.static.elab-plus.com/miniprogram/sharefriend/originazre.png"></image>
                    <view class="userInformation-container-organize-content-text">
                        <text>组织信息</text>
                    </view>
                </view>
            </view>
        </view>
        <view class="wg-box">
            <view class="wg-title">
            围观人数 <span style="color:#ac7b5b">{{currentNum}}</span>人
        </view>
            <view class="wg-head-box">
                <image class="wg-head" src="https://dm.static.elab-plus.com/miniprogram/memberlist_empty.png" wx:if="{{headList.length==0}}"></image>
                <image class="wg-head" src="{{item.headPortrait&&item.headPortrait.length>0?item.headPortrait:'../../image/wepy_pro/default-head.png'}}" wx:for="{{headList}}" wx:key="index"></image>
                <view class="mencen" wx:if="{{headList.length>14}}"></view>
            </view>
            <view class="head-text" wx:if="{{isGetHeadList}}">
                <text class="head-t" wx:if="{{headList.length>13}}">悄悄告诉你，有这么多好友浏览了你分享的小程序</text>
                <text class="head-t" wx:if="{{headList.length>0&&headList.length<14}}">只有这几个好友浏览了你分享的小程序，再接再厉</text>
                <text class="head-t" wx:if="{{headList.length==0}}">这么好的小程序居然还没有分享出去，快去打破0围观惨案</text>
            </view>
        </view>
        <view class="recommend-list">
            <view class="recommend-list-title">
                <view bindtap="ClkRecommend" class="recommend-list-title-recommend" style="{{showRecommend?'padding-bottom: 4px;color: #030303;':'opacity: 0.5;color: #121212;'}}{{!showPhoneModel&&showRecommend&&baobeiTableView?'border-bottom: 1px solid #2359CC;':''}}">
                    <text>我的推荐</text>
                </view>
                <view bindtap="ClkBaobei" class="recommend-list-title-baobei" style="{{showBaoBei?'border-bottom: 1px solid #2359CC;padding-bottom: 4px;':'opacity: 0.5;color: #121212;'}}" wx:if="{{!showPhoneModel&&baobeiTableView}}">
                    <text>我的报备</text>
                </view>
            </view>
            <view class="recommend-list-noPhone" wx:if="{{showPhoneModel&&recommendList.length>0&&specialUser}}">
