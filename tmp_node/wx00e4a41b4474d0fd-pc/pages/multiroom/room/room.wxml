<myLoading id="myLoading"></myLoading>
<navbar data="{{navbar}}"></navbar>
<view class="container-box" style="background-image:{{'url('+houseBgImg+')'}}">
    <view style="width:100vw;margin-left:0px;padding-top:0px;">
        <rtc-room assistant="{{assistant}}" beauty="{{beauty}}" bind:decline="decline" bind:onliveplay="onliveplay" bind:trigVideo="trigVideo" bindonRoomEvent="onRoomEvent" bindstatechange="onPush" debug="{{debug}}" flag="{{flag}}" flagStatus="{{flagStatus}}" hideBg="{{hideBg}}" houseBgImg="{{houseBgImg}}" id="rtcroom" muted="{{muted}}" roomID="{{roomID}}" roomInfo="{{roomname}}" startTime="{{startTime}}" template="1v3"></rtc-room>
    </view>
    <view class="operate" wx:if="{{hideBg==false}}">
        <image bindtap="decline" src="../../../image/wepy_pro/decline.png"></image>
    </view>
    <view class="head-view" wx:if="{{assistant&&assistant.head&&hideBg==false}}">
        <image class="head" src="{{assistant.head}}"></image>
    </view>
    <view class="headTips" wx:if="{{hideBg==false}}">看房顾问{{assistant.name||''}}正在连接中···</view>
    <view class="evaluate" wx:if="{{evaluateFlag=='1'}}">
        <view class="bg" style="background-image:{{'url('+houseBgImg+')'}}"></view>
        <view class="backView height1">
            <image class="headImg" src="{{assistant.head||'https://dm.static.elab-plus.com/elab-backstage/avatar_default1.png'}}"></image>
            <image bindtap="realExit" class="close" src="../../../image/wepy_pro/close.png"></image>
            <view class="word1">您的评价会让我们做的更好</view>
            <view class="word2">您有吐槽吗？诚挚邀请您如实填写感受</view>
            <view class="content">
                <view class="word3 uniteWordStyle row">
					您对看房体验满意吗
					<image class="anonymous" src="../../../image/wepy_pro/anonymous.png"></image>
                </view>
                <view class="selectList row {{goodIndex?'':'unselected'}}">
                    <view bindtap="handleIndex" class="selectItem right60 row {{goodIndex=='1'?' active':''}}" data-index="1">
                        <image class="iconSelect1" src="{{goodIndex=='1'?'../../../image/wepy_pro/icon-good-select.png':'../../../image/wepy_pro/icon-good-unselect.png'}}"></image>
						满意
					</view>
                    <view bindtap="handleIndex" class="selectItem row {{goodIndex=='2'?' active':''}}" data-index="2">
                        <image class="iconSelect2" src="{{goodIndex=='2'?'../../../image/wepy_pro/icon-bad-select.png':'../../../image/wepy_pro/icon-bad-unselect.png'}}"></image>
						不满意
					</view>
                </view>
                <view class="word4 uniteWordStyle row" wx:if="{{goodIndex=='1'}}">
					您如何评价视频看房
					<image class="anonymous" src="../../../image/wepy_pro/anonymous.png"></image>
                </view>
                <view class="selectList row" wx:if="{{goodIndex=='1'}}">
                    <view bindtap="handleIndex1" class="selectItem right15 row {{goodIndexList1[0]==1?' active':''}}" data-index="1">
						顾问热情
					</view>
                    <view bindtap="handleIndex1" class="selectItem right15 row {{goodIndexList1[1]==1?' active':''}}" data-index="2">
						体验流畅
					</view>
                    <view bindtap="handleIndex1" class="selectItem row {{goodIndexList1[2]==1?' active':''}}" data-index="3">
						效率很高
					</view>
                </view>
                <view class="selectList row  unselected " wx:if="{{goodIndex=='1'}}">
                    <view bindtap="handleIndex1" class="selectItem right15 row {{goodIndexList1[3]==1?' active':''}}" data-index="4">
						功能好用
					</view>
                    <view bindtap="handleIndex1" class="selectItem row {{goodIndexList1[4]==1?' active':''}}" data-index="5">
						快捷便利
					</view>
                </view>
                <view class="word4 uniteWordStyle row" wx:if="{{goodIndex=='2'}}">
					您体验中存在什么问题
					<image class="anonymous" src="../../../image/wepy_pro/anonymous.png"></image>
                </view>
                <view class="selectList row" wx:if="{{goodIndex=='2'}}">
                    <view bindtap="handleIndex2" class="selectItem right15 row {{goodIndexList2[0]==1?' active':''}}" data-index="1">
						画质不佳
					</view>
                    <view bindtap="handleIndex2" class="selectItem right15 row {{goodIndexList2[1]==1?' active':''}}" data-index="2">
						隐私顾虑
					</view>
                    <view bindtap="handleIndex2" class="selectItem row {{goodIndexList2[2]==1?' active':''}}" data-index="3">
						网络卡顿
					</view>
                </view>
                <view class="selectList row {{goodIndexList2[4]==1?'':'unselected'}}" wx:if="{{goodIndex=='2'}}">
                    <view bindtap="handleIndex2" class="selectItem right15 row {{goodIndexList2[3]==1?' active':''}}" data-index="4">
						服务不佳
					</view>
                    <view bindtap="handleIndex2" class="selectItem row {{goodIndexList2[4]==1?' active':''}}" data-index="5">
						其他原因
					</view>
                </view>
                <textarea bindblur="bindTextAreaBlur" class="textarea" placeholder="请输入您的感受…" placeholderClass="placeholder" wx:if="{{goodIndex=='2'&&goodIndexList2[4]==1}}"></textarea>
            </view>
            <view bindtap="submitEvaluate" class="btn {{goodIndex=='1'&&goodIndex1!=''||goodIndex=='2'&&goodIndex2!=''?' active':''}}">完成评价</view>
        </view>
    </view>
</view>
