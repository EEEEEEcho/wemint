    font-size: 28rpx;
}

.iconSelect1 {
    width: 36rpx;
    height: 37rpx;
    margin-right: 15rpx;
}

.iconSelect2 {
    width: 36rpx;
    height: 38rpx;
    margin-right: 15rpx;
}

.btn {
