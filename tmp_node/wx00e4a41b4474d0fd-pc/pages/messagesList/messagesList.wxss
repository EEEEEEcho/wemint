.title-box {
    width: 100%;
    height: 70px;
    display: flex;
    box-sizing: border-box;
    padding: 40px 20px 40px 20px;
    justify-content: space-between;
    align-items: center;
}

.title-text {
    line-height: 50px;
    font-size: 32px;
    font-weight: bold;
    height: 50px;
}

.title-right {
    height: 35px;
    width: 50px;
    overflow: hidden;
}

.bind-icon {
    position: absolute;
    right: -120rpx;
    top: 3px;
    width: 103.5rpx;
    height: 30rpx;
}

.title-right image {
    height: 35px;
    width: 50px;
}

.message-box {
    border-bottom: 1px solid #d8d8d8;
    box-sizing: border-box;
    margin-left: 20px;
    width: calc(100% - 20px);
    height: 80px;
    display: flex;
    padding-right: 20px;
    align-items: center;
    justify-content: flex-start;
}

.message-right-box {
    height: 50px;
    flex: 1;
}

.message-right-top {
    display: flex;
    justify-content: space-between;
}

.last-message-time {
    font-size: 12px;
    color: #999999;
}

.name {
    margin-left: 10px;
    font-weight: bold;
    position: relative;
}

.name-text {
    white-space: nowrap;
    display: inline-block;
    overflow: hidden;
    text-overflow: ellipsis;
    max-width: 125px;
}

.message-right-bottom {
    font-size: 15px;
    margin-left: 10px;
    color: #7684a0;
}

.message-text {
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    font-size: 14px;
    width: 250px;
}

.message-box .head-box {
    position: relative;
    width: 43px;
    height: 43px;
}

.head-box .newRed {
    z-index: 20;
    width: 15px;
    height: 15px;
    color: #fff;
    top: -5px;
    right: 1px;
    font-size: 12px;
    border-radius: 50%;
    background: red;
    text-align: center;
    line-height: 15px;
    position: absolute;
}

.head-box image {
    border-radius: 100%;
    width: 100%;
    height: 100%;
}

.more-adviser {
    height: 60px;
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    border-top: 1px solid #fff;
    margin-top: 13px;
}

.more-adviser image {
    width: 326px;
    height: 31px;
}

.loadData {
    width: 100px;
    height: 100px;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    background: rgba(0,0,0,0.6);
    border-radius: 10px;
    z-index: 1000;
    display: flex;
    align-items: center;
    justify-content: center;
}

.fixImg {
    width: 100%;
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 10;
}

.mask {
    position: fixed;
    top: 0px;
    left: 0px;
    width: 100vw;
    height: 100vh;
    background-color: rgba(155,155,155,0.6);
    z-index: 15;
}

.model {
    position: absolute;
    width: 80vw;
    left: 10vw;
    top: 20vh;
    border-radius: 20rpx;
    background-color: white;
    z-index: 16;
}

.model .modelTitle {
    width: 100%;
    padding: 20rpx 0rpx;
    font-size: 40rpx;
    text-align: center;
    color: block;
    border-bottom: 1rpx solid rgb(237,237,237);
}

.model .modelBody {
    box-sizing: border-box;
    width: 100%;
    padding: 50rpx 50rpx;
    text-align: center;
    color: rgb(178,178,178);
    font-size: 30rpx;
    border-bottom: 1rpx solid rgb(237,237,237);
}

.model .btns {
    width: 100%;
    text-align: center;
}

.model .btns .agree {
    border-width: 0px;
    color: rgb(26,172,25);
}

.no-adviser-box {
    width: 100vw;
    text-align: center;
    margin-top: 20vh;
}

.no-adviser-text {
    font-size: 30rpx;
    color: #5370FF;
}

.ic-phone {
    width: 12px;
    display: inline-block;
    background-size: 100% 100%;
    margin-right: 10rpx;
    background-image: url('https://dm.static.elab-plus.com/wepy_pro/im/ic_boda.png');
    height: 12px;
}

.myload {
    position: absolute;
    color: rgb(235,237,239);
    z-index: 10;
    width: 190rpx;
    left: 277rpx;
    top: 400rpx;
    opacity: 0.95;
    background: #363636;
    text-align: center;
    font-size: 24rpx;
    padding: 30rpx;
    border-radius: 16rpx;
    box-sizing: border-box;
}

.myload .spinner {
    display: inline-block;
    margin-bottom: 20rpx;
    width: 30rpx;
    height: 30rpx;
    background-color: white;
    animation: rotateplane 1.5s infinite ease-in-out;
}

.tel-button {
    line-height: 40px;
    font-size: 15px;
    border-radius: 10px;
    width: 120px;
    height: 40px;
    position: absolute;
    bottom: 10vh;
    background-color: #5472fc;
    color: #Fff;
    left: 50%;
    transform: translateX(-50%);
}