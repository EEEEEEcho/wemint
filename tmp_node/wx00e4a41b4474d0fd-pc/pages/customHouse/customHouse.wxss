.space-left-bottom {
    left: 200rpx;
    top: 700rpx;
}

.space-name {
    padding: 0 20rpx;
    color: #fff;
    font-size: 24rpx;
    line-height: 50rpx;
    border-radius: 25rpx 0 0 25rpx;
    background: rgba(42,69,59,.7);
}

.space-name-top {
    border-radius: 0 25rpx 25rpx 0;
}

.space-plus {
    width: 50rpx;
    height: 50rpx;
    margin-left: 20rpx;
}

.space-plus-left {
    margin-left: 0;
    margin-right: 20rpx;
}

.input-box {
    flex: auto;
    display: flex;
    background: #D5D5D5;
}

.input {
    flex: auto;
    margin: 12rpx 10rpx;
    height: 76rpx;
    line-height: 76rpx;
    background: #fff;
    padding-left: 72rpx;
}

.input-icon {
    position: absolute!important;
    top: 20rpx;
    left: 20rpx;
    width: 60rpx;
    height: 60rpx;
}

.btn-send {
    width: 200rpx;
    height: inherit;
    text-align: center;
    color: #fff;
    background-image: linear-gradient(-206deg,#5B5B5B 0%,#12120F 100%);
    font-size: 18px;
}

.comment-area {
    position: absolute;
    top: 50%;
    left: 45rpx;
    width: 100%;
    max-width: 70%;
    height: 50%;
    font-size: 28rpx;
    display: flex;
    flex-direction: column;
    overflow-y: auto;
}

.comment-unfold {
    top: 814rpx;
}

.comment-title {
    display: table;
    color: #fff;
    margin-bottom: 20rpx;
    padding-right: 30rpx;
    background: rgba(151,151,151,.904);
    border-radius: 6rpx;
    font-size: 14px;
    line-height: 50rpx;
    padding: 0 10rpx;
}

.icon-arrow {
    width: 14px;
    height: 14px;
    margin: 5px 0 0 2px;
}

.comment-item {
    display: table;
    color: #fff;
    line-height: 50rpx;
    padding: 0 10rpx;
    margin-bottom: 30rpx;
    border-radius: 6rpx;
    background: rgba(155,155,155,.7);
}

.icon-close {
    width: 28rpx;
    height: 28rpx;
    margin-left: 20rpx;
    vertical-align: middle;
}

.black {
    color: #fff;
    justify-content: center;
    background: linear-gradient(197deg,rgba(91,91,91,1) 0%,rgba(18,18,15,1) 100%);
}

.flex-1 {
    flex: 1;
    text-align: center;
}