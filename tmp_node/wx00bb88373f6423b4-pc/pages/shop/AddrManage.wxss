.addr-manage-container {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    font-size: 28rpx;
    padding-bottom: 60rpx;
    box-sizing: border-box;
    z-index: 99999;
    height: 100%;
    background-color: rgba(0,0,0,.7);
}

.input {
    width: 575rpx;
    height: 96rpx;
    display: flex;
    align-items: center;
}

.pickerBox {
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
}

.manage-containerBg {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 2;
}

.noplaceImg {
    width: 50%;
    margin: 120px auto 0;
}

.addr-manage-box {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 998rpx;
    z-index: 3;
    background-color: #f4f4f4;
}

.addr-manage-title {
    position: relative;
    height: 88rpx;
    line-height: 88rpx;
    text-align: center;
}

.addr-manage-back {
    position: absolute;
    right: 0;
    top: 0;
    height: 88rpx;
    line-height: 88rpx;
    width: 100rpx;
    text-align: center;
    font-size: 32rpx;
    color: #666;
}

.addr-manage-titleContent {
    position: absolute;
    bottom: 0;
    left: 0;
    height: 998rpx;
    width: 100%;
    z-index: 3;
    background-color: #fff;
}

.addr-manage-btnadd {
    position: fixed;
    left: 0;
    bottom: 0;
    text-align: center;
    width: 100%;
    background: #ff7200;
    color: white;
    height: 98rpx;
    line-height: 98rpx;
    border-radius: 0rpx;
    font-size: 32rpx;
}

.addr-manage-editdlg .editdlg-item {
    display: flex;
    height: 96rpx;
    align-items: center;
}

.addr-manage-editdlg .title.place {
    flex: 0 0 50%;
}

.addr-manage-editdlg .title {
    flex: 0 0 25%;
    display: flex;
    align-items: center;
    height: 96rpx;
    padding-left: 26rpx;
    font-size: 30rpx;
    color: #333;
    box-sizing: border-box;
}

.addr-manage-editdlg .data {
    flex: 1;
    display: flex;
    align-items: center;
    height: 96rpx;
}

.addr-manage-editdlg .editdlg-item.default .data {
    justify-content: flex-end;
    padding-right: 26rpx;
    box-sizing: border-box;
}

.addr-manage-editdlg .data input,.addr-manage-editdlg .data input {
    display: flex;
    width: 100%;
    flex: 1;
    padding-right: 26rpx;
    box-sizing: border-box;
    height: 96rpx;
}

.addr-manage-btnsave {
    position: fixed;
    left: 0;
    bottom: 0;
    text-align: center;
    width: 100%;
    background: #ff7200;
    color: white;
    height: 98rpx;
    line-height: 98rpx;
    border-radius: 0rpx;
    font-size: 32rpx;
}

.addr-manage-list {
    margin: 0rpx 26rpx;
    background-color: #f4f4f4;
}

.addr-manage-addritem {
    margin-top: 26rpx;
    background-color: #fff;
    box-sizing: border-box;
    position: relative;
}

.addr-manage-addritem:last-child {
    margin-bottom: 20rpx;
}

.addr-manage-header {
    position: relative;
    display: flex;
    padding: 30rpx 22rpx 30rpx 0;
    box-sizing: border-box;
}

.handleDelAndAmend {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 20rpx 26rpx;
    font-size: 26rpx;
}

.handleDelAndAmend .selectBox {
    flex: 0 0 40%;
    color: #666;
}

.handleDelAndAmend .handleDelAndAmendRight {
    flex: 1;
    display: flex;
    justify-content: flex-end;
    align-items: center;
    flex-direction: row;
    color: #666;
}

.handleDelAndAmendRight view {
    display: flex;
    font-size: 26rpx;
    align-items: center;
}

.handleDelAndAmend .handleDelAndAmendRight text {
    color: #666;
    display: inline-block;
    margin-right: 12rpx;
    font-size: 40rpx;
}

.handleDelAndAmendRight view:first-child {
    margin-right: 50rpx;
}

.approveSele {
    position: absolute;
    right: 0;
    top: 0;
    width: 0;
    height: 0;
    border: 32rpx solid #ff7200;
    border-left-color: transparent;
    border-bottom-color: transparent;
}

.approveSele .iconfont {
    position: absolute;
    top: -23rpx;
    right: -24rpx;
    font-size: 26rpx;
    color: #fff;
}

.iconPlace {
    flex: 0 0 60rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.addr-manage-content {
    flex: 1;
}

.addr-manage-content .nameAndTel {
    font-size: 26rpx;
    color: #333;
}

.addr-manage-content .nameAndTel .realName {
    display: inline-block;
    margin-right: 30rpx;
}

.addr-manage-content .placeText {
    font-size: 24rpx;
    color: #999;
    margin-top: 12rpx;
}

.placeTextWidth {
    width: 80%;
}