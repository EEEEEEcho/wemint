page {
    background: #fff;
}

.browose .browose-time {
    width: 100%;
    height: 76rpx;
    background: #f0f0f0;
    font-size: 26rpx;
    color: #999;
    padding: 0 26rpx;
    line-height: 76rpx;
}

.browose-item {
    display: flex;
    flex-direction: row;
}

.browose .browose-item-l {
    padding-bottom: 20rpx;
}

.browose .browose-l-img {
    width: 248rpx;
    height: 248rpx;
    background: #dbdbdb;
    margin-right: 22rpx;
    overflow: hidden;
}

.browose-r-tit {
    padding-top: 15px;
    padding-right: 26rpx;
    font-size: 28rpx;
    line-height: 40rpx;
    color: #333333;
}

.browose-item-r {
    position: relative;
    width: 560rpx;
}

.browose-r-price {
    font-size: 30rpx;
    font-weight: bold;
    color: #333333;
    position: absolute;
    bottom: 20rpx;
}

.browose-empty {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    background: #fff;
    text-align: center;
    position: fixed;
    bottom: 70rpx;
    right: 70rpx;
    box-shadow: 0 0 20rpx rgba(97,97,97,.2);
    z-index: 99;
    display: flex;
    align-items: center;
    flex-direction: column;
    justify-content: center;
}

.browose-empty .icon-iconfont_delete {
    font-size: 30rpx;
    color: #666666;
}

.browose-empty .browose-empty-txt {
    font-size: 20rpx;
    color: #666666;
}

.browose-l-img image {
    width: 100%;
}