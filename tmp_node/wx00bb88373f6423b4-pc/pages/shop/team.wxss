.teamWrapper .title {
    height: 70rpx;
    line-height: 70rpx;
    background-color: #f9f9f9;
    padding-left: 26rpx;
    box-sizing: border-box;
    font-size: 26rpx;
    color: #666;
}

.active {
    display: block!important;
}

.teamWrapper .distList {
    background-color: #fff;
}

.putu {
    display: none;
}

.icon-huiyuanyiji {
    display: inline-block;
    color: #fcd23d;
    font-size: 40rpx;
    padding-right: 26rpx;
}

.icon-huiyuanerji {
    display: inline-block;
    color: #d9d9d9;
    font-size: 40rpx;
    padding-right: 26rpx;
}

.icon-huiyuansanji {
    display: inline-block;
    color: #f7a118;
    font-size: 40rpx;
    padding-right: 26rpx;
}

.icon-huiyuan1 {
    display: inline-block;
    color: #ff7d1e;
    font-size: 40rpx;
    padding-right: 26rpx;
}

.teamWrapper .distList .memberItem {
    position: relative;
    align-items: center;
    flex-direction: row;
    padding-left: 26rpx;
    line-height: 88rpx;
    box-sizing: border-box;
    font-size: 30rpx;
    color: #333;
    height: 88rpx;
}

.show {
    display: none;
}

.teamWrapper .distList .memberItem .number {
    position: absolute;
    right: 18rpx;
    top: 0;
    height: 88rpx;
    display: flex;
    align-items: center;
}

.meRightIcon {
    display: inline-block;
    margin-left: 14rpx;
    font-size: 30rpx;
}