page {
    display: flex;
    flex-direction: column;
    width: 100%;
    font-family: -apple-system-font,'Helvetica Neue',Helvetica,'Microsoft YaHei',sans-serif;
}

view,text,button,input {
    font-size: 28rpx;
}

