<view class="dialog-container" wx:if="{{isAuthor}}">
    <view class="dialog-mask"></view>
    <view class="dialog-info">
        <view class="dialog-title">登录提示</view>
        <view class="dialog-content">小程序需要您的授权才能提供更好的服务哦</view>
        <view class="dialog-footer">
            <button bindgetuserinfo="getUserInfo" class="dialog-btn" disabled="{{canClick}}" openType="getUserInfo">知道了</button>
        </view>
    </view>
</view>
<web-view src="{{projectUrl}}/wallet/userDepositCard/home/{{merchantConfigId}}?smallOpenId={{smallOpenId}}&time={{time}}" wx:else></web-view>
