.cardBox {
    width: 300px;
    height: 150px;
    border-radius: 8px;
    background: #666;
    margin: 20px auto;
}

.cardName {
    color: #fff;
    font-size: 18px;
    padding-top: 20px;
    padding-left: 24px;
    line-height: 20px;
    float: left;
    width: 190px;
}

.viphycard {
    color: #fff;
    font-size: 14px;
    padding-right: 24px;
    float: right;
    line-height: 20px;
    padding-top: 110px;
}

.cardNumber {
    float: right;
    padding-right: 24px;
    font-size: 14px;
    padding-top: 60px;
    color: #fff;
}

.nameBox {
    border-bottom: 1px #666 solid;
    padding-bottom: 38px;
    width: 100%;
}

.name {
    border-left: 3px #ffbf20 solid;
    padding-left: 20px;
    margin-left: 36px;
    float: left;
    font-size: 16px;
    color: #fff;
    box-sizing: border-box;
    width: 80px;
}

.nameInput {
    float: left;
    margin-left: 10px;
    font-size: 14px;
    color: #666;
    width: 180px;
}

.weui-input {
    font-size: 14px;
    color: #fff;
    float: left;
}

.sexBox {
    border-bottom: 1px #666 solid;
    padding-bottom: 38px;
    margin-top: 16px;
}

.sex {
    border-left: 4px #ffbf20 solid;
    padding-left: 20px;
    margin-left: 36px;
    float: left;
    font-size: 16px;
    color: #fff;
}

.sexRadio {
    float: left;
    margin-left: 6px;
    color: #fff;
    font-size: 14px;
}

.birthBox {
    border-bottom: 1px #666 solid;
    padding-bottom: 38px;
    margin-top: 16px;
}

.birth {
    border-left: 4px #ffbf20 solid;
    padding-left: 20px;
    margin-left: 36px;
    float: left;
    font-size: 16px;
    color: #fff;
}

radio {
    transform: scale(0.5);
}

.ageBox {
    border-bottom: 1px #666 solid;
    padding-bottom: 38px;
    margin-top: 16px;
    color: #fff;
}

.age {
    border-left: 4px #ffbf20 solid;
    padding-left: 20px;
    margin-left: 36px;
    float: left;
    font-size: 16px;
    color: #fff;
}

.telBox {
    border-bottom: 1px #666 solid;
    padding-bottom: 38px;
    margin-top: 16px;
}

.tel {
    border-left: 4px #ffbf20 solid;
    padding-left: 20px;
    margin-left: 36px;
    float: left;
    font-size: 16px;
    color: #fff;
}

.telInput {
    float: left;
    margin-left: 10px;
    font-size: 14px;
    color: #666;
    width: 180px;
}

.lqhyk {
    background: #777;
    color: #ffbf20;
    width: 86%;
    margin: 60px auto;
    letter-spacing: 8px;
}

radio .wx-radio-input {
    border-radius: 50%;
    width: 40rpx;
    height: 40rpx;
    border: 1px #666 solid;
}

radio .wx-radio-input.wx-radio-input-checked {
    border: none;
    background: #ffbf20;
}

radio .wx-radio-input.wx-radio-input-checked::before {
    border-radius: 50%;
    width: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    font-size: 30rpx;
    color: #fff;
    background: transparent;
    transform: translate(-50%,-50%) scale(1);
}

.ad_popError {
    background: #de352d;
    color: #fff;
    height: 58rpx;
    line-height: 58rpx;
    font-size: 24rpx;
    text-align: center;
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    z-index: 3;
}

.cover {
    position: fixed;
    top: 0;
    left: 0;
    width: 100vw;
    height: 100vh;
    background-color: rgba(0,0,0,0.4);
    z-index: 2;
}

.cover1 {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 3;
    background-color: transparent;
    width: 100vw;
    height: 100vh;
}

.cover3 {
    position: fixed;
    width: 80vw;
    height: 60vw;
    z-index: 4;
    background-color: #666;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    border-radius: 6px;
    display: flex;
    justify-content: space-between;
    flex-direction: column;
    padding: 12vw 0;
    box-sizing: border-box;
}

.evaluation {
    background: #777;
    color: #ffbf20;
    width: 86%;
    margin: 0 auto;
}