.IntroTitle {
    color: #ffbf20;
    border-left: 3px #ffbf00 solid;
    padding-left: 20px;
    margin: 10px 0;
    margin-left: 20px;
    font-size: 16px;
    line-height: 18px;
}

.IntroTextbox {
    box-sizing: border-box;
    width: 100%;
    margin-top: 10px;
    font-size: 18px;
}

.IntroText {
    font-size: 16px;
    color: #999;
}

.editableGraphic {
    width: calc(100% - 40px);
    margin: 0 auto;
    box-sizing: border-box;
    background: #333;
    padding: 15px 20px;
    border-radius: 6px;
}

.setMealImg {
    width: 100%;
    display: block;
    height: calc(50vw - 40px);
    border-radius: 6px;
}

.setMealTextBox {
    box-sizing: border-box;
    width: 100%;
    display: block;
    padding-top: 10px;
    padding-bottom: 10px;
    color: #9b9b9b;
}

.setMealText {
    box-sizing: border-box;
    font-size: 16px;
    line-height: 20px;
}