.cover {
    position: fixed;
    top: 0;
    left: 0;
    width: 100vw;
    height: 100vh;
    background-color: rgba(0,0,0,0.4);
    z-index: 2;
}

.cover1 {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 3;
    background-color: transparent;
    width: 100vw;
    height: 100vh;
}

.cover2 {
    position: fixed;
    width: 80vw;
    height: 40vw;
    z-index: 4;
    background-color: #666;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    border-radius: 6px;
}

.row1 {
    display: flex;
    justify-content: space-between;
    box-sizing: border-box;
    padding: 20px 20px 10px;
}

.row0 {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 30px;
    color: #ffbf20;
