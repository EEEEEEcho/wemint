.waitText {
    padding-left: 0!important;
}

.waitInfo {
    border-left: 4px #ffbf20 solid;
    padding-left: 20px;
    color: #fff;
    margin-top: 24px;
    margin-left: 38px;
}

.destineInfo {
    box-sizing: border-box;
    color: #999;
    width: 90%;
    margin: 15px auto;
    border-radius: 5px;
    padding: 20px;
    line-height: 25px;
    font-size: 16px;
    background-color: #333;
}

.destineButton {
    background: #444;
    color: #999;
    border-radius: 8px;
    width: 80%;
}