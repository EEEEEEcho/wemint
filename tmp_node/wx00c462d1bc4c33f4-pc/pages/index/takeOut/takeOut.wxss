.goods {
    display: flex;
    position: absolute;
    width: 100%;
    top: 0;
    height: calc(100vh - 70px);
    overflow: hidden;
    font-family: "微软雅黑";
    background: #3d3d3d;
}

.goods .menu-wrapper {
    flex: 0 0 80px;
    width: 80px;
    background: #383838;
    height: calc(100vh - 70px);
}

.goods .menu-wrapper .menu-item {
    display: table;
    width: 56px;
    padding: 0 0 0 12px;
}

.goods .menu-wrapper .menu-item:current .text {
    border: none;
}

.goods .menu-wrapper .menu-item .icon {
    display: inline-block;
    width: 12px;
    height: 12px;
    vertical-align: top;
    margin-right: 4px;
    background-size: 12px 12px;
    background-repeat: no-repeat;
}

.goods .menu-wrapper .menu-item .text {
    display: block;
    width: 56px;
    line-height: 50px;
    height: 50px;
    position: relative;
    font-size: 12px;
    color: #ccc;
    white-space: nowrap;
    overflow: hidden;
}

.goods .foods-wrapper {
    flex: 1;
    height: calc(100vh - 70px);
    padding-bottom: 10px;
    box-sizing: border-box;
}

.goods .foods-wrapper .food-list .food-item {
    box-sizing: border-box;
    display: flex;
    margin: 0 18px;
    padding-top: 10px;
    position: relative;
}

.goods .foods-wrapper .food-list .food-item .icon {
    flex: 0 0 57px;
    margin-right: 10px;
}

.goods .foods-wrapper .food-list .food-item .content {
    flex: 1;
    margin-left: 5px;
}

.goods .foods-wrapper .food-list .food-item .content .name {
    margin-top: 2px;
    font-size: 14px;
    line-height: 14px;
    height: 14px;
    color: #eee;
}

.goods .foods-wrapper .food-list .food-item .content .desc,.extra {
    font-size: 10px;
    line-height: 10px;
    color: rgb(147,153,159);
}

.goods .foods-wrapper .food-list .food-item .content .desc {
    margin-bottom: 8px;
    line-height: 12px;
}

.goods .foods-wrapper .food-list .food-item .content .extra .count {
    margin-right: 12px;
    color: #999;
}

.goods .foods-wrapper .food-list .food-item .content .price {
    box-sizing: border-box;
    display: block;
    height: 40px;
    align-items: center;
}

.goods .foods-wrapper .food-list .food-item .content .price .now {
    margin-right: 8px;
    font-size: 14px;
    color: #fb4e44;
    line-height: 40px;
}

.goods .foods-wrapper .food-list .food-item .content .price .old {
    font-size: 10px;
    color: rgb(147,153,159);
    text-decoration: line-through;
}

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper {
    position: absolute;
    right: 0;
    bottom: 4px;
}

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper .cartControl {
    font-size: 0;
}

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper .cartControl .cart-decrease,.cart-add {
    display: inline-block;
    padding: 6px 0;
}

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper .cartControl .cart-decrease,.cart-add :fade-enter-active,:fade-leave-active {
    transition: all 0.4s linear;
}

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper .cartControl .cart-decrease,.cart-add :fade-enter,:fade-leave-active {
    opacity: 0 transform translate3d(24px,0,0);
}

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper .cartControl .cart-decrease,.cart-add .inner {
    display: inline-block;
    line-height: 24px;
    font-size: 24px;
    vertical-align: top;
    color: #ffbf21;
}

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper .cartControl .cart-decrease,.cart-add :inner-enter-active,:inner-leave-active {
    transition: all 0.4s linear;
    transform: rotate(0);
}

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper .cartControl .cart-decrease,.cart-add :inner-enter,:inner-leave-active {
    opacity: 0 transform rotate(180deg);
}

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper .cartControl .cart-count {
    display: inline-block;
    font-size: 14px;
    line-height: 24px;
    width: 30px;
    vertical-align: top;
    padding-top: 6px;
    text-align: center;
    color: rgb(147,153,159);
}

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper .cartControl .cart-add {
    display: inline-block;
    padding: 6px 0;
    line-height: 24px;
    font-size: 24px;
    vertical-align: top;
    color: rgb(0,160,220,0.2);
}

