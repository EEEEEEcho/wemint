.swiper-container {
    width: 100%;
    padding: 0 5vw;
    box-sizing: border-box;
    height: 45vw;
    clear: both;
    transform: translate3d(0,0,0);
    overflow: hidden;
    position: relative;
}

swiper {
    width: 100%;
    height: 45vw;
}

swiper-item {
    box-sizing: border-box;
}

.swiper-container .swiper .img {
    width: 100%;
    height: 45vw;
    display: block;
    margin: 0 auto;
    border-radius: 8px;
}

.video {
