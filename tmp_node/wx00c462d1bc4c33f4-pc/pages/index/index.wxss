    height: 100vh;
    position: fixed;
    top: 0;
    left: 0;
    background-color: rgba(0,0,0,0.8);
    z-index: 11;
}

.container-mask {
    height: calc(100vh - 55px);
    width: 100%;
    padding: 50rpx 0;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.radiu6 {
