.yhtjs {
    border-bottom: 1px #666 solid;
}

.yhtjBox {
    margin-top: 20px;
    width: 100%;
}

.yhtjC {
    color: #fff;
    border-left: 4px #ffbf00 solid;
    padding-left: 20px;
    margin-left: 10px;
}

.yhtjSlide {
    height: calc(234rpx + 15px);
    width: 100%;
    display: flex;
    margin: 0 auto;
}

.yhtjSlide_Item {
    box-sizing: border-box;
    flex: 0 0 auto;
    padding-left: 20rpx;
}

.yhtjSlide_Item_img {
    width: 350rpx;
    height: 234rpx;
    float: left;
    border-radius: 8px;
}

.yhtjSlide_Item_shade {
    box-sizing: border-box;
    position: relative;
    bottom: 20px;
    height: 20px;
    background: #333;
    width: 100%;
    line-height: 20px;
    opacity: 0.9;
    font-size: 14px;
    color: #fff;
    padding: 0 3%;
    overflow: hidden;
    border-bottom-left-radius: 8px;
    border-bottom-right-radius: 8px;
}

.yhtjSlide_Item_text {
    float: left;
    line-height: 20px;
    font-size: 10px;
    color: #fff;
}

.yhtjSlide_Item_price {
    float: right;
    line-height: 20px;
    font-size: 10px;
    color: #fff;
}

.wntjBox {
    margin-top: 20px;
    width: 94%;
}

.left-box {
    width: 50%;
    margin-top: 20px;
    display: flex;
    justify-content: flex-end;
    margin-right: 10rpx;
}

.right-box {
    width: 50%;
    margin-top: 20px;
    display: flex;
    justify-content: flex-start;
    margin-left: 10rpx;
}

.wntjC {
    color: #fff;
    border-left: 4px #ffbf00 solid;
    padding-left: 20px;
    margin-left: 10px;
}

.wntjpicBox_Item_shade1,.wntjpicBox_Item_shade2,.wntjpicBox_Item_shade3,.wntjpicBox_Item_shade4 {
    float: left;
    position: absolute;
    bottom: 260px;
    height: 20px;
    background: #333;
    line-height: 20px;
    opacity: 0.9;
    color: #fff;
    padding: 0 3%;
    overflow: hidden;
    border-radius: 0 10px 10px 0;
}

.wntjpicBox_Item_text1,.wntjpicBox_Item_text2 {
    float: left;
    line-height: 20px;
    font-size: 12px;
    color: #fff;
}

.wntjpicBox_Item_shade2 {
    bottom: 75px;
}

.wntjpicBox_Item_shade3 {
    bottom: 260px;
}

.wntjpicBox_Item_shade4 {
